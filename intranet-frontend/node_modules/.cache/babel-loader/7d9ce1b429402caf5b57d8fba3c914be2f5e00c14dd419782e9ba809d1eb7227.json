{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nimport { ref, onMounted } from 'vue';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  setup() {\n    const pendingCount = ref(0);\n    const showAdminButton = ref(false);\n    const fetchAprobacionesCount = async () => {\n      try {\n        const response = await axios.get('http://localhost:8000/api/listar_aprobaciones_dpto');\n        if (response.data.success) {\n          pendingCount.value = response.data.data.length;\n        }\n      } catch (error) {\n        console.error('Error fetching aprobaciones count:', error);\n      }\n    };\n    const checkIfAdmin = async () => {\n      try {\n        const response = await axios.get('http://localhost:8000/api/roles_usuario', {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('auth_token')}`\n          }\n        });\n        if (response.data.roles && response.data.roles.includes('Administrador')) {\n          showAdminButton.value = true;\n        } else {\n          showAdminButton.value = false;\n        }\n      } catch (error) {\n        console.error('Error checking user roles:', error);\n      }\n    };\n    onMounted(() => {\n      fetchAprobacionesCount();\n      checkIfAdmin();\n    });\n    return {\n      pendingCount,\n      showAdminButton,\n      fetchAprobacionesCount,\n      checkIfAdmin\n    };\n  },\n  methods: {\n    paginaPrincipal() {\n      this.$router.push({\n        name: 'homeinicio'\n      });\n    },\n    paginaMiEspacio() {\n      this.$router.push({\n        name: 'miespacio'\n      });\n    },\n    paginaPortalColaboradores() {\n      this.$router.push({\n        name: 'portalcolaboradores'\n      });\n    },\n    paginaPortalAprobaciones() {\n      this.$router.push({\n        name: 'portalaprobaciones'\n      });\n    },\n    paginaAdministradores() {\n      this.$router.push({\n        name: 'administradores'\n      });\n    },\n    enConstruccion() {\n      Swal.fire({\n        icon: 'warning',\n        title: 'En construcción',\n        text: 'Esta sección está en construcción. Por favor, vuelve más tarde.'\n      });\n    },\n    async cerrarSesion() {\n      try {\n        const token = localStorage.getItem('auth_token');\n        if (!token) {\n          console.error('No se encontró el token en el localStorage');\n          return;\n        }\n        await axios.post('http://localhost:8000/api/logout', {}, {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        localStorage.removeItem('auth_token');\n        this.$router.push({\n          name: 'login'\n        });\n      } catch (error) {\n        console.error('Error al cerrar sesión', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","ref","onMounted","FontAwesomeIcon","components","setup","pendingCount","showAdminButton","fetchAprobacionesCount","response","get","data","success","value","length","error","console","checkIfAdmin","headers","localStorage","getItem","roles","includes","methods","paginaPrincipal","$router","push","name","paginaMiEspacio","paginaPortalColaboradores","paginaPortalAprobaciones","paginaAdministradores","enConstruccion","fire","icon","title","text","cerrarSesion","token","post","removeItem"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Header.vue"],"sourcesContent":["<template>\r\n  <nav class=\"navbar navbar-expand-sm navbar-light bg-white border-bottom box-shadow bg-body-tertiary\">\r\n    <div class=\"container-fluid\">\r\n      <a class=\"navbar-brand\" href=\"#\" @click=\"enConstruccion\">UAF INTRANET</a>\r\n      <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNavDropdown\" aria-controls=\"navbarNavDropdown\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n        <span class=\"navbar-toggler-icon\"></span>\r\n      </button>\r\n      <div class=\"collapse navbar-collapse\" id=\"navbarNavDropdown\">\r\n        <ul class=\"navbar-nav\">\r\n          <li class=\"nav-item\">\r\n            <a class=\"nav-link active\" aria-current=\"page\" href=\"#\" @click=\"paginaMiEspacio\">Mi espacio</a>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a class=\"nav-link\" href=\"#\" @click=\"paginaPortalColaboradores\">Portal de colaboradores</a>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a class=\"nav-link\" href=\"#\" @click=\"paginaPortalAprobaciones\">\r\n              Portal de aprobaciones\r\n              <span v-if=\"pendingCount > 0\" class=\"badge bg-danger\">{{ pendingCount }}</span>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n      <form class=\"d-flex\" role=\"search\">\r\n        <button v-if=\"showAdminButton\" class=\"btn btn-outline-dark me-4\" type=\"button\" @click=\"paginaAdministradores\">Administradores <font-awesome-icon :icon=\"['fas', 'user-secret']\" /></button>\r\n        <button class=\"btn btn-outline-danger me-2\" type=\"button\" @click=\"cerrarSesion\">Desconectarse</button>\r\n      </form>\r\n    </div>\r\n  </nav>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\nimport { ref, onMounted } from 'vue';\r\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n\r\nexport default {\r\n  components: {\r\n    FontAwesomeIcon\r\n  },\r\n  setup() {\r\n    const pendingCount = ref(0);\r\n    const showAdminButton = ref(false);\r\n\r\n    const fetchAprobacionesCount = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/listar_aprobaciones_dpto');\r\n        if (response.data.success) {\r\n          pendingCount.value = response.data.data.length;\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching aprobaciones count:', error);\r\n      }\r\n    };\r\n\r\n    const checkIfAdmin = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/roles_usuario', {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('auth_token')}`\r\n          }\r\n        });\r\n        if (response.data.roles && response.data.roles.includes('Administrador')) {\r\n          showAdminButton.value = true;\r\n        } else {\r\n          showAdminButton.value = false;\r\n        }\r\n      } catch (error) {\r\n        console.error('Error checking user roles:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchAprobacionesCount();\r\n      checkIfAdmin();\r\n    });\r\n\r\n    return {\r\n      pendingCount,\r\n      showAdminButton,\r\n      fetchAprobacionesCount,\r\n      checkIfAdmin\r\n    };\r\n  },\r\n  methods: {\r\n    paginaPrincipal() {\r\n      this.$router.push({ name: 'homeinicio' });\r\n    },\r\n    paginaMiEspacio() {\r\n      this.$router.push({ name: 'miespacio' });\r\n    },\r\n    paginaPortalColaboradores() {\r\n      this.$router.push({ name: 'portalcolaboradores' });\r\n    },\r\n    paginaPortalAprobaciones() {\r\n      this.$router.push({ name: 'portalaprobaciones' });\r\n    },\r\n    paginaAdministradores() {\r\n      this.$router.push({ name: 'administradores' });\r\n    },\r\n    enConstruccion() {\r\n      Swal.fire({\r\n        icon: 'warning',\r\n        title: 'En construcción',\r\n        text: 'Esta sección está en construcción. Por favor, vuelve más tarde.',\r\n      });\r\n    },\r\n    async cerrarSesion() {\r\n      try {\r\n        const token = localStorage.getItem('auth_token');\r\n        if (!token) {\r\n          console.error('No se encontró el token en el localStorage');\r\n          return;\r\n        }\r\n\r\n        await axios.post('http://localhost:8000/api/logout', {}, {\r\n          headers: {\r\n            'Authorization': `Bearer ${token}`\r\n          }\r\n        });\r\n\r\n        localStorage.removeItem('auth_token');\r\n\r\n        this.$router.push({ name: 'login' });\r\n      } catch (error) {\r\n        console.error('Error al cerrar sesión', error);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.navbar {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,YAAW,GAAIL,GAAG,CAAC,CAAC,CAAC;IAC3B,MAAMM,eAAc,GAAIN,GAAG,CAAC,KAAK,CAAC;IAElC,MAAMO,sBAAqB,GAAI,MAAAA,CAAA,KAAY;MACzC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,oDAAoD,CAAC;QACtF,IAAID,QAAQ,CAACE,IAAI,CAACC,OAAO,EAAE;UACzBN,YAAY,CAACO,KAAI,GAAIJ,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACG,MAAM;QAChD;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D;IACF,CAAC;IAED,MAAME,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,MAAMR,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,yCAAyC,EAAE;UAC1EQ,OAAO,EAAE;YACP,eAAe,EAAG,UAASC,YAAY,CAACC,OAAO,CAAC,YAAY,CAAE;UAChE;QACF,CAAC,CAAC;QACF,IAAIX,QAAQ,CAACE,IAAI,CAACU,KAAI,IAAKZ,QAAQ,CAACE,IAAI,CAACU,KAAK,CAACC,QAAQ,CAAC,eAAe,CAAC,EAAE;UACxEf,eAAe,CAACM,KAAI,GAAI,IAAI;QAC9B,OAAO;UACLN,eAAe,CAACM,KAAI,GAAI,KAAK;QAC/B;MACF,EAAE,OAAOE,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF,CAAC;IAEDb,SAAS,CAAC,MAAM;MACdM,sBAAsB,CAAC,CAAC;MACxBS,YAAY,CAAC,CAAC;IAChB,CAAC,CAAC;IAEF,OAAO;MACLX,YAAY;MACZC,eAAe;MACfC,sBAAsB;MACtBS;IACF,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAa,CAAC,CAAC;IAC3C,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAY,CAAC,CAAC;IAC1C,CAAC;IACDE,yBAAyBA,CAAA,EAAG;MAC1B,IAAI,CAACJ,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAsB,CAAC,CAAC;IACpD,CAAC;IACDG,wBAAwBA,CAAA,EAAG;MACzB,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC;QAAEC,