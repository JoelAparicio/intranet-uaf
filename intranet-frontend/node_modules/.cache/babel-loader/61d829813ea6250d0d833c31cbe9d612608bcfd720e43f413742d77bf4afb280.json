{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  data() {\n    return {\n      solicitudesAceptadas: [],\n      loading: true,\n      role: '',\n      userRoles: []\n    };\n  },\n  methods: {\n    async fetchUserRole() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\n        this.userRoles = response.data.roles || [];\n        this.determineRole();\n      } catch (error) {\n        console.error('Error fetching user role:', error);\n      }\n    },\n    determineRole() {\n      const RolesJefes = ['Jefe de Tecnología', 'Jefe de Relaciones Públicas', 'Jefe de Administración', 'Jefe de Análisis Estratégico', 'Jefe de Análisis Operativo', 'Jefe de Asesoría Legal', 'Jefe de Contact Center', 'Jefe de Cooperación Nacional e Internacional'];\n      const RolesRRHH = ['Jefe de Recursos Humanos', 'Recursos Humanos'];\n      if (RolesJefes.some(r => this.userRoles.includes(r))) {\n        this.role = 'jefe_departamento';\n        this.fetchSolicitudesAceptadasDepartamento();\n      } else if (RolesRRHH.some(r => this.userRoles.includes(r))) {\n        this.role = 'rrhh';\n        this.fetchSolicitudesAceptadasRRHH();\n      } else {\n        console.log('No matching role found');\n      }\n    },\n    fetchSolicitudesAceptadasDepartamento() {\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-departamento').then(response => {\n        if (response.data.success) {\n          this.solicitudesAceptadas = response.data.data;\n        }\n        this.loading = false;\n      }).catch(error => {\n        console.error('Error fetching solicitudes aceptadas:', error);\n        this.loading = false;\n      });\n    },\n    fetchSolicitudesAceptadasRRHH() {\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-rrhh').then(response => {\n        if (response.data.success) {\n          this.solicitudesAceptadas = response.data.data;\n        }\n        this.loading = false;\n      }).catch(error => {\n        console.error('Error fetching solicitudes aceptadas:', error);\n        this.loading = false;\n      });\n    },\n    formatFecha(fecha) {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    }\n  },\n  mounted() {\n    this.fetchUserRole();\n  }\n};","map":{"version":3,"names":["axios","moment","FontAwesomeIcon","components","data","solicitudesAceptadas","loading","role","userRoles","methods","fetchUserRole","response","get","roles","determineRole","error","console","RolesJefes","RolesRRHH","some","r","includes","fetchSolicitudesAceptadasDepartamento","fetchSolicitudesAceptadasRRHH","log","then","success","catch","formatFecha","fecha","format","mounted"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Solicitudes_Aceptadas.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-0\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header\">\r\n        <h4 class=\"text-center\">Solicitudes Aceptadas</h4>\r\n      </div>\r\n      <div class=\"card-body pb-0\" style=\"max-height: 700px; overflow-y: auto;\">\r\n        <table class=\"table table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th>ID Solicitud</th>\r\n              <th>Colaborador</th>\r\n              <th>Tipo de Solicitud</th>\r\n              <th>Fecha de Creación</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-if=\"solicitudesAceptadas.length === 0 && !loading\">\r\n              <td colspan=\"4\" class=\"text-center\">\r\n                No has aceptado solicitudes <font-awesome-icon :icon=\"['far', 'face-smile-wink']\" />\r\n              </td>\r\n            </tr>\r\n            <tr v-else v-for=\"solicitud in solicitudesAceptadas\" :key=\"solicitud.id_solicitud\">\r\n              <td>{{ solicitud.id_solicitud }}</td>\r\n              <td>{{ solicitud.colaborador }}</td>\r\n              <td>{{ solicitud.tipo_solicitud }}</td>\r\n              <td>{{ formatFecha(solicitud.fecha_creacion) }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport 'moment/locale/es';\r\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n\r\nexport default {\r\n  components: {\r\n    FontAwesomeIcon\r\n  },\r\n  data() {\r\n    return {\r\n      solicitudesAceptadas: [],\r\n      loading: true,\r\n      role: '',\r\n      userRoles: []\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchUserRole() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\r\n        this.userRoles = response.data.roles || [];\r\n        this.determineRole();\r\n      } catch (error) {\r\n        console.error('Error fetching user role:', error);\r\n      }\r\n    },\r\n    determineRole() {\r\n      const RolesJefes = [\r\n        'Jefe de Tecnología',\r\n        'Jefe de Relaciones Públicas',\r\n        'Jefe de Administración',\r\n        'Jefe de Análisis Estratégico',\r\n        'Jefe de Análisis Operativo',\r\n        'Jefe de Asesoría Legal',\r\n        'Jefe de Contact Center',\r\n        'Jefe de Cooperación Nacional e Internacional'\r\n      ];\r\n\r\n      const RolesRRHH = [\r\n        'Jefe de Recursos Humanos',\r\n        'Recursos Humanos'\r\n      ];\r\n\r\n      if (RolesJefes.some(r => this.userRoles.includes(r))) {\r\n        this.role = 'jefe_departamento';\r\n        this.fetchSolicitudesAceptadasDepartamento();\r\n      } else if (RolesRRHH.some(r => this.userRoles.includes(r))) {\r\n        this.role = 'rrhh';\r\n        this.fetchSolicitudesAceptadasRRHH();\r\n      } else {\r\n        console.log('No matching role found');\r\n      }\r\n    },\r\n    fetchSolicitudesAceptadasDepartamento() {\r\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-departamento')\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            this.solicitudesAceptadas = response.data.data;\r\n          }\r\n          this.loading = false;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching solicitudes aceptadas:', error);\r\n          this.loading = false;\r\n        });\r\n    },\r\n    fetchSolicitudesAceptadasRRHH() {\r\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-rrhh')\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            this.solicitudesAceptadas = response.data.data;\r\n          }\r\n          this.loading = false;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching solicitudes aceptadas:', error);\r\n          this.loading = false;\r\n        });\r\n    },\r\n    formatFecha(fecha) {\r\n      if (!fecha) return '';\r\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchUserRole();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin-top: 20px;\r\n}\r\n.card {\r\n  border: 1px solid #dee2e6;\r\n  border-radius: 5px;\r\n  padding: 15px;\r\n}\r\n.card-header {\r\n  border-bottom: 1px solid #dee2e6;\r\n}\r\nh4 {\r\n  margin-bottom: 0;\r\n}\r\n.table th, .table td {\r\n  vertical-align: middle;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAO,kBAAkB;AACzB,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,oBAAoB,EAAE,EAAE;MACxBC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,yCAAyC,CAAC;QAC3E,IAAI,CAACJ,SAAQ,GAAIG,QAAQ,CAACP,IAAI,CAACS,KAAI,IAAK,EAAE