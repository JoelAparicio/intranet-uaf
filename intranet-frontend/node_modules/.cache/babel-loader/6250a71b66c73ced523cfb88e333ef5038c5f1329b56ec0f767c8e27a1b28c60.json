{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport Swal from 'sweetalert2';\nexport default {\n  data() {\n    return {\n      aprobaciones: []\n    };\n  },\n  methods: {\n    fetchAprobaciones() {\n      axios.get('http://localhost:8000/api/listar_aprobaciones_dpto').then(response => {\n        if (response.data.success) {\n          this.aprobaciones = response.data.data;\n        }\n      }).catch(error => {\n        console.error('Error fetching aprobaciones:', error);\n      });\n    },\n    aprobarSolicitud(id_solicitud) {\n      axios.post(`http://localhost:8000/api/aprobar_solicitud/${id_solicitud}`).then(response => {\n        if (response.data.success) {\n          this.fetchAprobaciones();\n        }\n      }).catch(error => {\n        console.error('Error approving solicitud:', error);\n      });\n    },\n    rechazarSolicitud(id_solicitud) {\n      Swal.fire({\n        title: 'Ingrese los comentarios para el rechazo:',\n        input: 'textarea',\n        inputAttributes: {\n          'aria-label': 'Ingrese los comentarios para el rechazo'\n        },\n        showCancelButton: true,\n        confirmButtonText: 'Rechazar',\n        cancelButtonText: 'Cancelar',\n        showLoaderOnConfirm: true,\n        preConfirm: comentarios_jefe => {\n          if (!comentarios_jefe) {\n            Swal.showValidationMessage('Debe ingresar un comentario para el rechazo');\n          } else {\n            return axios.post(`http://localhost:8000/api/rechazar_solicitud/${id_solicitud}`, {\n              comentarios_jefe\n            }).then(response => {\n              if (!response.data.success) {\n                throw new Error(response.data.message || 'Error rechazando la solicitud');\n              }\n              return response.data;\n            }).catch(error => {\n              Swal.showValidationMessage(`Request failed: ${error}`);\n            });\n          }\n        },\n        allowOutsideClick: () => !Swal.isLoading()\n      }).then(result => {\n        if (result.isConfirmed) {\n          Swal.fire({\n            title: 'Rechazado!',\n            text: 'La solicitud ha sido rechazada.',\n            icon: 'success'\n          });\n          this.fetchAprobaciones();\n        }\n      });\n    },\n    visualizarSolicitud(id_solicitud) {\n      // Lógica para visualizar la solicitud (puede ser una redirección a una vista detallada o mostrar un modal)\n      console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\n    },\n    formatFecha(fecha) {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    }\n  },\n  mounted() {\n    this.fetchAprobaciones();\n  }\n};","map":{"version":3,"names":["axios","moment","Swal","data","aprobaciones","methods","fetchAprobaciones","get","then","response","success","catch","error","console","aprobarSolicitud","id_solicitud","post","rechazarSolicitud","fire","title","input","inputAttributes","showCancelButton","confirmButtonText","cancelButtonText","showLoaderOnConfirm","preConfirm","comentarios_jefe","showValidationMessage","Error","message","allowOutsideClick","isLoading","result","isConfirmed","text","icon","visualizarSolicitud","log","formatFecha","fecha","format","mounted"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Lista_Aprobaciones.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n      <h2>Lista de Aprobaciones</h2>\r\n      <table class=\"table table-striped\">\r\n        <thead>\r\n          <tr>\r\n            <th>ID Solicitud</th>\r\n            <th>Colaborador</th>\r\n            <th>Tipo de Solicitud</th>\r\n            <th>Fecha de Creación</th>\r\n            <th>Acciones</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"aprobacion in aprobaciones\" :key=\"aprobacion.id_solicitud\">\r\n            <td>{{ aprobacion.id_solicitud }}</td>\r\n            <td>{{ aprobacion.jefe_departamento }}</td>\r\n            <td>{{ aprobacion.tipo_solicitud }}</td>\r\n            <td>{{ formatFecha(aprobacion.fecha_creacion) }}</td>\r\n            <td>\r\n              <button class=\"btn btn-success btn-sm\" @click=\"aprobarSolicitud(aprobacion.id_solicitud)\">\r\n                <i class=\"fas fa-check\"></i>\r\n              </button>\r\n              <button class=\"btn btn-danger btn-sm\" @click=\"rechazarSolicitud(aprobacion.id_solicitud)\">\r\n                <i class=\"fas fa-times\"></i>\r\n              </button>\r\n              <button class=\"btn btn-info btn-sm\" @click=\"visualizarSolicitud(aprobacion.id_solicitud)\">\r\n                <i class=\"fas fa-eye\"></i>\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import moment from 'moment';\r\n  import 'moment/locale/es';\r\n  import Swal from 'sweetalert2';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        aprobaciones: [],\r\n      };\r\n    },\r\n    methods: {\r\n      fetchAprobaciones() {\r\n        axios.get('http://localhost:8000/api/listar_aprobaciones_dpto')\r\n          .then(response => {\r\n            if (response.data.success) {\r\n              this.aprobaciones = response.data.data;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching aprobaciones:', error);\r\n          });\r\n      },\r\n      aprobarSolicitud(id_solicitud) {\r\n        axios.post(`http://localhost:8000/api/aprobar_solicitud/${id_solicitud}`)\r\n          .then(response => {\r\n            if (response.data.success) {\r\n              this.fetchAprobaciones();\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Error approving solicitud:', error);\r\n          });\r\n      },\r\n      rechazarSolicitud(id_solicitud) {\r\n        Swal.fire({\r\n          title: 'Ingrese los comentarios para el rechazo:',\r\n          input: 'textarea',\r\n          inputAttributes: {\r\n            'aria-label': 'Ingrese los comentarios para el rechazo'\r\n          },\r\n          showCancelButton: true,\r\n          confirmButtonText: 'Rechazar',\r\n          cancelButtonText: 'Cancelar',\r\n          showLoaderOnConfirm: true,\r\n          preConfirm: (comentarios_jefe) => {\r\n            if (!comentarios_jefe) {\r\n              Swal.showValidationMessage('Debe ingresar un comentario para el rechazo');\r\n            } else {\r\n              return axios.post(`http://localhost:8000/api/rechazar_solicitud/${id_solicitud}`, { comentarios_jefe })\r\n                .then(response => {\r\n                  if (!response.data.success) {\r\n                    throw new Error(response.data.message || 'Error rechazando la solicitud');\r\n                  }\r\n                  return response.data;\r\n                })\r\n                .catch(error => {\r\n                  Swal.showValidationMessage(`Request failed: ${error}`);\r\n                });\r\n            }\r\n          },\r\n          allowOutsideClick: () => !Swal.isLoading()\r\n        }).then((result) => {\r\n          if (result.isConfirmed) {\r\n            Swal.fire({\r\n              title: 'Rechazado!',\r\n              text: 'La solicitud ha sido rechazada.',\r\n              icon: 'success'\r\n            });\r\n            this.fetchAprobaciones();\r\n          }\r\n        });\r\n      },\r\n      visualizarSolicitud(id_solicitud) {\r\n        // Lógica para visualizar la solicitud (puede ser una redirección a una vista detallada o mostrar un modal)\r\n        console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\r\n      },\r\n      formatFecha(fecha) {\r\n        if (!fecha) return '';\r\n        return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchAprobaciones();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .container {\r\n    margin-top: 20px;\r\n  }\r\n  .table th, .table td {\r\n    vertical-align: middle;\r\n  }\r\n  .btn {\r\n    margin-right: 5px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAqCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAO,kBAAkB;AACzB,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClBN,KAAK,CAACO,GAAG,CAAC,oDAAoD,EAC3DC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACN,IAAI,CAACO,OAAO,EAAE;UACzB,IAAI,CAACN,YAAW,GAAIK,QAAQ,CAACN,IAAI,CAACA,IAAI;QACxC;MACF,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;MACtD,CAAC,CAAC;IACN,CAAC;IACDE,gBAAgBA,CAACC,YAAY,EAAE;MAC7Bf,KAAK,CAACgB,IAAI,CAAE,+CAA8CD,YAAa,EAAC,EACrEP,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACN,IAAI,CAACO,OAAO,EAAE;UACzB,IAAI,CAACJ,iBAAiB,CAAC,CAAC;QAC1B;MACF,CAAC,EACAK,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD,CAAC,CAAC;IACN,CAAC;IACDK,iBAAiBA,CAACF,YAAY,EAAE;MAC9Bb,IAAI,CAACgB,IAAI,CAAC;QACRC,KAAK,EAAE,0CAA0C;QACjDC,KAAK,EAAE,UAAU;QACjBC,eAAe,EAAE;UACf,YAAY,EAAE;QAChB,CAAC;QACDC,gBAAgB,EAAE,IAAI;QACtBC,iBAAiB,EAAE,UAAU;QAC7BC,gBAAgB,EAAE,UAAU;QAC5BC,mBAAmB,EAAE,IAAI;QACzBC,UAAU,EAAGC,gBAAgB,IAAK;UAChC,IAAI,CAACA,gBAAgB,EAAE;YACrBzB,IAAI,CAAC0B,qBAAqB,CAAC,6CAA6C,CAAC;UAC3E,OAAO;YACL,OAAO5B,KAAK,CAACgB,IAAI,CAAE,gDAA+CD,YAAa,EAAC,EAAE;cAAEY;YAAiB,CAAC,EACnGnB,IAAI,CAACC,QAAO,IAAK;cAChB,IAAI,CAACA,QAAQ,CAACN,IAAI,CAACO,OAAO,EAAE;gBAC1B,MAAM,IAAImB,KAAK,CAACpB,QAAQ,CAACN,IAAI,CAAC2B,OAAM,IAAK,+BAA+B,CAAC;cAC3E;cACA,OAAOrB,QAAQ,CAACN,IAAI;YACtB,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;cACdV,IAAI,CAAC0B,qBAAqB,CAAE,mBAAkBhB,KAAM,EAAC,CAAC;YACxD,CAAC,CAAC;UACN;QACF,CAAC;QACDmB,iBAAiB,EAAEA,CAAA,KAAM,CAAC7B,IAAI,CAAC8B,SAAS,CAAC;MAC3C,CAAC,CAAC,CAACxB,IAAI,CAAEyB,MAAM,IAAK;QAClB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACtBhC,IAAI,CAACgB,IAAI,CAAC;YACRC,KAAK,EAAE,YAAY;YACnBgB,IAAI,EAAE,iCAAiC;Y