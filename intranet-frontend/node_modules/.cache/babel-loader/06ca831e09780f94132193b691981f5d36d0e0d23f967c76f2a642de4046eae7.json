{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport Swal from 'sweetalert2';\nimport { ref, onMounted, watch } from 'vue';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  setup() {\n    const aprobaciones = ref([]);\n    const loading = ref(true);\n    const visualizada = ref(false);\n    const userRoles = ref([]);\n    const role = ref('');\n    const RolesJefes = ['Jefe de Tecnología', 'Jefe de Relaciones Públicas', 'Jefe de Administración', 'Jefe de Análisis Estratégico', 'Jefe de Análisis Operativo', 'Jefe de Asesoría Legal', 'Jefe de Contact Center', 'Jefe de Cooperación Nacional e Internacional'];\n    const RolesRRHH = ['Jefe de Recursos Humanos', 'Recursos Humanos'];\n    const fetchUserRoles = async () => {\n      try {\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\n        if (response.data.success) {\n          userRoles.value = response.data.roles;\n          determineRole();\n        }\n      } catch (error) {\n        console.error('Error fetching user roles:', error);\n      }\n    };\n    const determineRole = () => {\n      if (RolesJefes.some(r => userRoles.value.includes(r))) {\n        role.value = 'jefe_departamento';\n        console.log('Role assigned:', role.value); // Añadido log\n        console.log('Comparing with:', 'jefe_departamento'); // Añadido log\n        fetchAprobaciones('http://localhost:8000/api/aprobaciones-departamento');\n      } else if (RolesRRHH.some(r => userRoles.value.includes(r))) {\n        role.value = 'rrhh';\n        console.log('Role assigned:', role.value); // Añadido log\n        console.log('Comparing with:', 'rrhh'); // Añadido log\n        fetchAprobaciones('http://localhost:8000/api/aprobaciones-rrhh');\n      } else {\n        console.log('Comparing with:', 'jefe_departamento'); // Añadido log\n      }\n    };\n    determineRole();\n    const fetchAprobaciones = async url => {\n      try {\n        console.log(`Fetching aprobaciones from ${url}`); // Log para verificar la URL\n        const response = await axios.get(url);\n        if (response.data.success) {\n          console.log('Fetched data:', response.data.data); // Log para verificar la data recibida\n          aprobaciones.value = response.data.data;\n        }\n        loading.value = false;\n        Swal.close();\n      } catch (error) {\n        console.error('Error fetching aprobaciones:', error);\n        loading.value = false;\n        Swal.close();\n      }\n    };\n    const aprobarSolicitud = async id_solicitud => {\n      if (!visualizada.value) {\n        Swal.fire({\n          title: 'Atención',\n          text: 'Debe visualizar la solicitud antes de aprobarla.',\n          icon: 'warning',\n          confirmButtonText: 'Ok'\n        });\n        return;\n      }\n      let url = '';\n      if (role.value === 'jefe_departamento') {\n        url = `http://localhost:8000/api/aprobar-solicitud/${id_solicitud}`;\n      } else if (role.value === 'rrhh') {\n        url = `http://localhost:8000/api/aprobar-solicitud-rrhh/${id_solicitud}`;\n      }\n      try {\n        const response = await axios.put(url);\n        if (response.data.success) {\n          Swal.fire({\n            title: 'Aprobado!',\n            text: 'La solicitud ha sido aprobada exitosamente.',\n            icon: 'success',\n            timer: 1000,\n            showConfirmButton: false\n          }).then(() => {\n            determineRole();\n          });\n        }\n      } catch (error) {\n        Swal.fire({\n          title: 'Error!',\n          text: `Error al aprobar la solicitud: ${error.response?.data?.message || error.message}`,\n          icon: 'error',\n          timer: 3000,\n          showConfirmButton: false\n        });\n        console.error('Error approving solicitud:', error);\n      }\n    };\n    const rechazarSolicitud = async id_solicitud => {\n      if (!visualizada.value) {\n        Swal.fire({\n          title: 'Atención',\n          text: 'Debe visualizar la solicitud antes de rechazarla.',\n          icon: 'warning',\n          confirmButtonText: 'Ok'\n        });\n        return;\n      }\n      let url = '';\n      let requestData = {};\n      if (role.value === 'jefe_departamento') {\n        url = `http://localhost:8000/api/rechazar-solicitud/${id_solicitud}`;\n        requestData = {\n          comentarios_jefe: ''\n        };\n      } else if (role.value === 'rrhh') {\n        url = `http://localhost:8000/api/rechazar-solicitud-rrhh/${id_solicitud}`;\n        requestData = {\n          comentarios_rrhh: ''\n        };\n      }\n      Swal.fire({\n        title: 'Ingrese el motivo para el rechazo:',\n        input: 'textarea',\n        inputAttributes: {\n          'aria-label': 'Ingrese el motivo para el rechazo'\n        },\n        showCancelButton: true,\n        confirmButtonText: 'Rechazar',\n        cancelButtonText: 'Cancelar',\n        showLoaderOnConfirm: true,\n        preConfirm: comentarios => {\n          if (!comentarios) {\n            Swal.showValidationMessage('Debe ingresar un motivo para el rechazo');\n          } else {\n            if (role.value === 'jefe_departamento') {\n              requestData.comentarios_jefe = comentarios;\n            } else if (role.value === 'rrhh') {\n              requestData.comentarios_rrhh = comentarios;\n            }\n            return axios.put(url, requestData).then(response => {\n              if (!response.data.success) {\n                throw new Error(response.data.message || 'Error rechazando la solicitud');\n              }\n              return response.data;\n            }).catch(error => {\n              Swal.showValidationMessage(`Request failed: ${error}`);\n            });\n          }\n        },\n        allowOutsideClick: () => !Swal.isLoading()\n      }).then(result => {\n        if (result.isConfirmed) {\n          Swal.fire({\n            title: 'Rechazado!',\n            text: 'La solicitud ha sido rechazada.',\n            icon: 'success',\n            timer: 1000,\n            showConfirmButton: false\n          }).then(() => {\n            determineRole();\n          });\n        }\n      });\n    };\n    const visualizarSolicitud = id_solicitud => {\n      console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\n      visualizada.value = true;\n    };\n    const formatFecha = fecha => {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    };\n    onMounted(() => {\n      fetchUserRoles();\n    });\n    return {\n      aprobaciones,\n      loading,\n      role,\n      visualizarSolicitud,\n      aprobarSolicitud,\n      rechazarSolicitud,\n      formatFecha\n    };\n  }\n};","map":{"version":3,"names":["axios","moment","Swal","ref","onMounted","watch","FontAwesomeIcon","components","setup","aprobaciones","loading","visualizada","userRoles","role","RolesJefes","RolesRRHH","fetchUserRoles","response","get","data","success","value","roles","determineRole","error","console","some","r","includes","log","fetchAprobaciones","url","close","aprobarSolicitud","id_solicitud","fire","title","text","icon","confirmButtonText","put","timer","showConfirmButton","then","message","rechazarSolicitud","requestData","comentarios_jefe","comentarios_rrhh","input","inputAttributes","showCancelButton","cancelButtonText","showLoaderOnConfirm","preConfirm","comentarios","showValidationMessage","Error","catch","allowOutsideClick","isLoading","result","isConfirmed","visualizarSolicitud","formatFecha","fecha","format"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Lista_Aprobaciones.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-0\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header\">\r\n        <h4 class=\"text-center\">Solicitudes pendientes</h4>\r\n      </div>\r\n      <div class=\"card-body pb-0\" style=\"max-height: 700px; overflow-y: auto;\">\r\n        <table class=\"table table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th>ID Solicitud</th>\r\n              <th>Colaborador</th>\r\n              <th>Tipo de Solicitud</th>\r\n              <th>Fecha de Creación</th>\r\n              <th>Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-if=\"aprobaciones.length === 0 && !loading\">\r\n              <td colspan=\"5\" class=\"text-center\">\r\n                No hay solicitudes pendientes <font-awesome-icon :icon=\"['far', 'face-smile-wink']\" />\r\n              </td>\r\n            </tr>\r\n            <tr v-else v-for=\"aprobacion in aprobaciones\" :key=\"aprobacion.id_solicitud\">\r\n              <td>{{ aprobacion.id_solicitud }}</td>\r\n              <td>{{ aprobacion.jefe_departamento }}</td>\r\n              <td>{{ aprobacion.tipo_solicitud }}</td>\r\n              <td>{{ formatFecha(aprobacion.fecha_creacion) }}</td>\r\n              <td>\r\n                <button class=\"btn btn-success btn-sm\" @click=\"aprobarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'check']\" />\r\n                </button>\r\n                <button class=\"btn btn-danger btn-sm\" @click=\"rechazarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'xmark']\" />\r\n                </button>\r\n                <button class=\"btn btn-info btn-sm\" @click=\"visualizarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'eye']\" />\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport 'moment/locale/es';\r\nimport Swal from 'sweetalert2';\r\nimport { ref, onMounted, watch } from 'vue';\r\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n\r\nexport default {\r\n  components: {\r\n    FontAwesomeIcon\r\n  },\r\n  setup() {\r\n    const aprobaciones = ref([]);\r\n    const loading = ref(true);\r\n    const visualizada = ref(false);\r\n    const userRoles = ref([]);\r\n    const role = ref('');\r\n\r\n    const RolesJefes = [\r\n      'Jefe de Tecnología',\r\n      'Jefe de Relaciones Públicas',\r\n      'Jefe de Administración',\r\n      'Jefe de Análisis Estratégico',\r\n      'Jefe de Análisis Operativo',\r\n      'Jefe de Asesoría Legal',\r\n      'Jefe de Contact Center',\r\n      'Jefe de Cooperación Nacional e Internacional'\r\n    ];\r\n\r\n    const RolesRRHH = [\r\n      'Jefe de Recursos Humanos',\r\n      'Recursos Humanos'\r\n    ];\r\n\r\n    const fetchUserRoles = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\r\n        if (response.data.success) {\r\n          userRoles.value = response.data.roles;\r\n          determineRole();\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching user roles:', error);\r\n      }\r\n    };\r\n\r\n    const determineRole = () => {\r\n      if (RolesJefes.some(r => userRoles.value.includes(r))) {\r\n        role.value = 'jefe_departamento';\r\n        console.log('Role assigned:', role.value); // Añadido log\r\n        console.log('Comparing with:', 'jefe_departamento'); // Añadido log\r\n        fetchAprobaciones('http://localhost:8000/api/aprobaciones-departamento');\r\n      } else if (RolesRRHH.some(r => userRoles.value.includes(r))) {\r\n        role.value = 'rrhh';\r\n        console.log('Role assigned:', role.value); // Añadido log\r\n        console.log('Comparing with:', 'rrhh'); // Añadido log\r\n        fetchAprobaciones('http://localhost:8000/api/aprobaciones-rrhh');\r\n      } else {\r\n        console.log('Comparing with:', 'jefe_departamento'); // Añadido log\r\n      }\r\n    };\r\n    determineRole();\r\n    const fetchAprobaciones = async (url) => {\r\n      try {\r\n        console.log(`Fetching aprobaciones from ${url}`); // Log para verificar la URL\r\n        const response = await axios.get(url);\r\n        if (response.data.success) {\r\n          console.log('Fetched data:', response.data.data); // Log para verificar la data recibida\r\n          aprobaciones.value = response.data.data;\r\n        }\r\n        loading.value = false;\r\n        Swal.close();\r\n      } catch (error) {\r\n        console.error('Error fetching aprobaciones:', error);\r\n        loading.value = false;\r\n        Swal.close();\r\n      }\r\n    };\r\n\r\n    const aprobarSolicitud = async (id_solicitud) => {\r\n      if (!visualizada.value) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de aprobarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n\r\n      let url = '';\r\n      if (role.value === 'jefe_departamento') {\r\n        url = `http://localhost:8000/api/aprobar-solicitud/${id_solicitud}`;\r\n      } else if (role.value === 'rrhh') {\r\n        url = `http://localhost:8000/api/aprobar-solicitud-rrhh/${id_solicitud}`;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.put(url);\r\n        if (response.data.success) {\r\n          Swal.fire({\r\n            title: 'Aprobado!',\r\n            text: 'La solicitud ha sido aprobada exitosamente.',\r\n            icon: 'success',\r\n            timer: 1000,\r\n            showConfirmButton: false\r\n          }).then(() => {\r\n            determineRole();\r\n          });\r\n        }\r\n      } catch (error) {\r\n        Swal.fire({\r\n          title: 'Error!',\r\n          text: `Error al aprobar la solicitud: ${error.response?.data?.message || error.message}`,\r\n          icon: 'error',\r\n          timer: 3000,\r\n          showConfirmButton: false\r\n        });\r\n        console.error('Error approving solicitud:', error);\r\n      }\r\n    };\r\n\r\n    const rechazarSolicitud = async (id_solicitud) => {\r\n      if (!visualizada.value) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de rechazarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n\r\n      let url = '';\r\n      let requestData = {};\r\n      if (role.value === 'jefe_departamento') {\r\n        url = `http://localhost:8000/api/rechazar-solicitud/${id_solicitud}`;\r\n        requestData = { comentarios_jefe: '' };\r\n      } else if (role.value === 'rrhh') {\r\n        url = `http://localhost:8000/api/rechazar-solicitud-rrhh/${id_solicitud}`;\r\n        requestData = { comentarios_rrhh: '' };\r\n      }\r\n\r\n      Swal.fire({\r\n        title: 'Ingrese el motivo para el rechazo:',\r\n        input: 'textarea',\r\n        inputAttributes: {\r\n          'aria-label': 'Ingrese el motivo para el rechazo'\r\n        },\r\n        showCancelButton: true,\r\n        confirmButtonText: 'Rechazar',\r\n        cancelButtonText: 'Cancelar',\r\n        showLoaderOnConfirm: true,\r\n        preConfirm: (comentarios) => {\r\n          if (!comentarios) {\r\n            Swal.showValidationMessage('Debe ingresar un motivo para el rechazo');\r\n          } else {\r\n            if (role.value === 'jefe_departamento') {\r\n              requestData.comentarios_jefe = comentarios;\r\n            } else if (role.value === 'rrhh') {\r\n              requestData.comentarios_rrhh = comentarios;\r\n            }\r\n\r\n            return axios.put(url, requestData)\r\n              .then(response => {\r\n                if (!response.data.success) {\r\n                  throw new Error(response.data.message || 'Error rechazando la solicitud');\r\n                }\r\n                return response