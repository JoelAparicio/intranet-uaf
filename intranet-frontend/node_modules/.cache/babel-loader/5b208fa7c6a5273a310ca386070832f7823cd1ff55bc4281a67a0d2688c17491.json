rtalaprobaciones' });\r\n      },\r\n      enConstruccion() {\r\n        Swal.fire({\r\n          icon: 'warning',\r\n          title: 'En construcción',\r\n          text: 'Esta sección está en construcción. Por favor, vuelve más tarde.',\r\n        });\r\n      },\r\n      async cerrarSesion() {\r\n        try {\r\n          const token = localStorage.getItem('auth_token');\r\n          if (!token) {\r\n            console.error('No se encontró el token en el localStorage');\r\n            return;\r\n          }\r\n  \r\n          await axios.post('http://localhost:8000/api/logout', {}, {\r\n            headers: {\r\n              'Authorization': `Bearer ${token}`\r\n            }\r\n          });\r\n  \r\n          localStorage.removeItem('auth_token');\r\n  \r\n          this.$router.push({ name: 'login' });\r\n        } catch (error) {\r\n          console.error('Error al cerrar sesión', error);\r\n        }\r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .navbar {\r\n    margin-bottom: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAmCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,YAAW,GAAIL,GAAG,CAAC,CAAC,CAAC;IAE3B,MAAMM,sBAAqB,GAAI,MAAAA,CAAA,KAAY;MACzC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,oDAAoD,CAAC;QACtF,IAAID,QAAQ,CAACE,IAAI,CAACC,OAAO,EAAE;UACzBL,YAAY,CAACM,KAAI,GAAIJ,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACG,MAAM;QAChD;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D;IACF,CAAC;IAEDZ,SAAS,CAAC,MAAM;MACdK,sBAAsB,CAAC,CAAC;IAC1B,CAAC,CAAC;IAEF,OAAO;MACLD,YAAY;MACZC;IACF,CAAC;EACH,CAAC;EACDS,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAa,CAAC,CAAC;IAC3C,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAY,CAAC,CAAC;IAC1C,CAAC;IACDE,yBAAyBA,CAAA,EAAG;MAC1B,IAAI,CAACJ,OAA