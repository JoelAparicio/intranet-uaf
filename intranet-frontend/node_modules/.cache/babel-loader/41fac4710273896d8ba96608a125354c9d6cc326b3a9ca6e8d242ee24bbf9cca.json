{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport Swal from 'sweetalert2';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  data() {\n    return {\n      aprobaciones: [],\n      loading: true,\n      visualizada: false,\n      role: ''\n    };\n  },\n  methods: {\n    async fetchUserRole() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\n        if (response.data.success) {\n          const roles = response.data.roles.map(role => role.normalize('NFKD').replace(/[\\u0300-\\u036F]/g, \"\"));\n          console.log('User roles:', roles); // Añade este log para verificar los roles\n          if (roles.includes('Jefe de Tecnologia') || roles.includes('Jefe de Relaciones Publicas') || roles.includes('Jefe de Administracion') || roles.includes('Jefe de Analisis Estrategico') || roles.includes('Jefe de Analisis Operativo') || roles.includes('Jefe de Asesoria Legal') || roles.includes('Jefe de Contact Center') || roles.includes('Jefe de Cooperacion Nacional e Internacional')) {\n            this.role = 'jefe_departamento';\n            this.fetchAprobacionesDepartamento();\n          } else if (roles.includes('Jefe de Recursos Humanos') || roles.includes('Recursos Humanos')) {\n            this.role = 'rrhh';\n            this.fetchAprobacionesRRHH();\n          }\n        }\n      } catch (error) {\n        console.error('Error fetching user role:', error);\n      }\n    },\n    fetchAprobacionesDepartamento() {\n      Swal.fire({\n        title: 'Cargando...',\n        allowOutsideClick: false,\n        didOpen: () => {\n          Swal.showLoading();\n        }\n      });\n      axios.get('http://localhost:8000/api/aprobaciones-departamento').then(response => {\n        console.log('Response data (Departamento):', response.data); // Añade este log para verificar los datos\n        if (response.data.success) {\n          this.aprobaciones = response.data.data;\n        }\n        this.loading = false;\n        Swal.close();\n      }).catch(error => {\n        console.error('Error fetching aprobaciones:', error);\n        this.loading = false;\n        Swal.close();\n      });\n    },\n    fetchAprobacionesRRHH() {\n      Swal.fire({\n        title: 'Cargando...',\n        allowOutsideClick: false,\n        didOpen: () => {\n          Swal.showLoading();\n        }\n      });\n      axios.get('http://localhost:8000/api/aprobaciones-rrhh').then(response => {\n        console.log('Response data (RRHH):', response.data); // Añade este log para verificar los datos\n        if (response.data.success) {\n          this.aprobaciones = response.data.data;\n        }\n        this.loading = false;\n        Swal.close();\n      }).catch(error => {\n        console.error('Error fetching aprobaciones:', error);\n        this.loading = false;\n        Swal.close();\n      });\n    },\n    aprobarSolicitud(id_solicitud) {\n      if (!this.visualizada) {\n        Swal.fire({\n          title: 'Atención',\n          text: 'Debe visualizar la solicitud antes de aprobarla.',\n          icon: 'warning',\n          confirmButtonText: 'Ok'\n        });\n        return;\n      }\n      let url = '';\n      if (this.role === 'jefe_departamento') {\n        url = `http://localhost:8000/api/aprobar-solicitud/${id_solicitud}`;\n      } else if (this.role === 'rrhh') {\n        url = `http://localhost:8000/api/aprobar-solicitud-rrhh/${id_solicitud}`;\n      }\n      axios.put(url).then(response => {\n        if (response.data.success) {\n          Swal.fire({\n            title: 'Aprobado!',\n            text: 'La solicitud ha sido aprobada exitosamente.',\n            icon: 'success',\n            timer: 1000,\n            showConfirmButton: false\n          }).then(() => {\n            this.fetchUserRole(); // Volver a cargar la lista\n          });\n        }\n      }).catch(error => {\n        Swal.fire({\n          title: 'Error!',\n          text: `Error al aprobar la solicitud: ${error.response?.data?.message || error.message}`,\n          icon: 'error',\n          timer: 3000,\n          showConfirmButton: false\n        });\n        console.error('Error approving solicitud:', error);\n      });\n    },\n    rechazarSolicitud(id_solicitud) {\n      if (!this.visualizada) {\n        Swal.fire({\n          title: 'Atención',\n          text: 'Debe visualizar la solicitud antes de rechazarla.',\n          icon: 'warning',\n          confirmButtonText: 'Ok'\n        });\n        return;\n      }\n      let url = '';\n      let requestData = {};\n      if (this.role === 'jefe_departamento') {\n        url = `http://localhost:8000/api/rechazar-solicitud/${id_solicitud}`;\n        requestData = {\n          comentarios_jefe: ''\n        };\n      } else if (this.role === 'rrhh') {\n        url = `http://localhost:8000/api/rechazar-solicitud-rrhh/${id_solicitud}`;\n        requestData = {\n          comentarios_rrhh: ''\n        };\n      }\n      Swal.fire({\n        title: 'Ingrese el motivo para el rechazo:',\n        input: 'textarea',\n        inputAttributes: {\n          'aria-label': 'Ingrese el motivo para el rechazo'\n        },\n        showCancelButton: true,\n        confirmButtonText: 'Rechazar',\n        cancelButtonText: 'Cancelar',\n        showLoaderOnConfirm: true,\n        preConfirm: comentarios => {\n          if (!comentarios) {\n            Swal.showValidationMessage('Debe ingresar un motivo para el rechazo');\n          } else {\n            if (this.role === 'jefe_departamento') {\n              requestData.comentarios_jefe = comentarios;\n            } else if (this.role === 'rrhh') {\n              requestData.comentarios_rrhh = comentarios;\n            }\n            return axios.put(url, requestData).then(response => {\n              if (!response.data.success) {\n                throw new Error(response.data.message || 'Error rechazando la solicitud');\n              }\n              return response.data;\n            }).catch(error => {\n              Swal.showValidationMessage(`Request failed: ${error}`);\n            });\n          }\n        },\n        allowOutsideClick: () => !Swal.isLoading()\n      }).then(result => {\n        if (result.isConfirmed) {\n          Swal.fire({\n            title: 'Rechazado!',\n            text: 'La solicitud ha sido rechazada.',\n            icon: 'success',\n            timer: 1000,\n            showConfirmButton: false\n          }).then(() => {\n            this.fetchUserRole(); // Volver a cargar la lista\n          });\n        }\n      });\n    },\n    visualizarSolicitud(id_solicitud) {\n      // Lógica para visualizar la solicitud (puede ser una redirección a una vista detallada o mostrar un modal)\n      console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\n      this.visualizada = true;\n    },\n    formatFecha(fecha) {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    }\n  },\n  mounted() {\n    this.fetchUserRole();\n  }\n};","map":{"version":3,"names":["axios","moment","Swal","FontAwesomeIcon","components","data","aprobaciones","loading","visualizada","role","methods","fetchUserRole","response","get","success","roles","map","normalize","replace","console","log","includes","fetchAprobacionesDepartamento","fetchAprobacionesRRHH","error","fire","title","allowOutsideClick","didOpen","showLoading","then","close","catch","aprobarSolicitud","id_solicitud","text","icon","confirmButtonText","url","put","timer","showConfirmButton","message","rechazarSolicitud","requestData","comentarios_jefe","comentarios_rrhh","input","inputAttributes","showCancelButton","cancelButtonText","showLoaderOnConfirm","preConfirm","comentarios","showValidationMessage","Error","isLoading","result","isConfirmed","visualizarSolicitud","formatFecha","fecha","format","mounted"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Lista_Aprobaciones.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-0\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header\">\r\n        <h4 class=\"text-center\">Solicitudes pendientes</h4>\r\n      </div>\r\n      <div class=\"card-body pb-0\" style=\"max-height: 700px; overflow-y: auto;\">\r\n        <table class=\"table table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th>ID Solicitud</th>\r\n              <th>Colaborador</th>\r\n              <th>Tipo de Solicitud</th>\r\n              <th>Fecha de Creación</th>\r\n              <th>Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-if=\"aprobaciones.length === 0 && !loading\">\r\n              <td colspan=\"5\" class=\"text-center\">\r\n                No hay solicitudes pendientes <font-awesome-icon :icon=\"['far', 'face-smile-wink']\" />\r\n              </td>\r\n            </tr>\r\n            <tr v-else v-for=\"aprobacion in aprobaciones\" :key=\"aprobacion.id_solicitud\">\r\n              <td>{{ aprobacion.id_solicitud }}</td>\r\n              <td>{{ aprobacion.jefe_departamento }}</td>\r\n              <td>{{ aprobacion.tipo_solicitud }}</td>\r\n              <td>{{ formatFecha(aprobacion.fecha_creacion) }}</td>\r\n              <td>\r\n                <button class=\"btn btn-success btn-sm\" @click=\"aprobarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'check']\" />\r\n                </button>\r\n                <button class=\"btn btn-danger btn-sm\" @click=\"rechazarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'xmark']\" />\r\n                </button>\r\n                <button class=\"btn btn-info btn-sm\" @click=\"visualizarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'eye']\" />\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport 'moment/locale/es';\r\nimport Swal from 'sweetalert2';\r\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n\r\nexport default {\r\n  components: {\r\n    FontAwesomeIcon\r\n  },\r\n  data() {\r\n    return {\r\n      aprobaciones: [],\r\n      loading: true,\r\n      visualizada: false,\r\n      role: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchUserRole() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\r\n        if (response.data.success) {\r\n          const roles = response.data.roles.map(role => role.normalize('NFKD').replace(/[\\u0300-\\u036F]/g, \"\"));\r\n          console.log('User roles:', roles); // Añade este log para verificar los roles\r\n          if (roles.includes('Jefe de Tecnologia') || roles.includes('Jefe de Relaciones Publicas') ||\r\n              roles.includes('Jefe de Administracion') || roles.includes('Jefe de Analisis Estrategico') ||\r\n              roles.includes('Jefe de Analisis Operativo') || roles.includes('Jefe de Asesoria Legal') ||\r\n              roles.includes('Jefe de Contact Center') || roles.includes('Jefe de Cooperacion Nacional e Internacional')) {\r\n            this.role = 'jefe_departamento';\r\n            this.fetchAprobacionesDepartamento();\r\n          } else if (roles.includes('Jefe de Recursos Humanos') || roles.includes('Recursos Humanos')) {\r\n            this.role = 'rrhh';\r\n            this.fetchAprobacionesRRHH();\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching user role:', error);\r\n      }\r\n    },\r\n    fetchAprobacionesDepartamento() {\r\n      Swal.fire({\r\n        title: 'Cargando...',\r\n        allowOutsideClick: false,\r\n        didOpen: () => {\r\n          Swal.showLoading();\r\n        }\r\n      });\r\n\r\n      axios.get('http://localhost:8000/api/aprobaciones-departamento')\r\n        .then(response => {\r\n          console.log('Response data (Departamento):', response.data); // Añade este log para verificar los datos\r\n          if (response.data.success) {\r\n            this.aprobaciones = response.data.data;\r\n          }\r\n          this.loading = false;\r\n          Swal.close();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching aprobaciones:', error);\r\n          this.loading = false;\r\n          Swal.close();\r\n        });\r\n    },\r\n    fetchAprobacionesRRHH() {\r\n      Swal.fire({\r\n        title: 'Cargando...',\r\n        allowOutsideClick: false,\r\n        didOpen: () => {\r\n          Swal.showLoading();\r\n        }\r\n      });\r\n\r\n      axios.get('http://localhost:8000/api/aprobaciones-rrhh')\r\n        .then(response => {\r\n          console.log('Response data (RRHH):', response.data); // Añade este log para verificar los datos\r\n          if (response.data.success) {\r\n            this.aprobaciones = response.data.data;\r\n          }\r\n          this.loading = false;\r\n          Swal.close();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching aprobaciones:', error);\r\n          this.loading = false;\r\n          Swal.close();\r\n        });\r\n    },\r\n    aprobarSolicitud(id_solicitud) {\r\n      if (!this.visualizada) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de aprobarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n      \r\n      let url = '';\r\n      if (this.role === 'jefe_departamento') {\r\n        url = `http://localhost:8000/api/aprobar-solicitud/${id_solicitud}`;\r\n      } else if (this.role === 'rrhh') {\r\n        url = `http://localhost:8000/api/aprobar-solicitud-rrhh/${id_solicitud}`;\r\n      }\r\n\r\n      axios.put(url)\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            Swal.fire({\r\n              title: 'Aprobado!',\r\n              text: 'La solicitud ha sido aprobada exitosamente.',\r\n              icon: 'success',\r\n              timer: 1000,\r\n              showConfirmButton: false\r\n            }).then(() => {\r\n              this.fetchUserRole(); // Volver a cargar la lista\r\n            });\r\n          }\r\n        })\r\n        .catch(error => {\r\n          Swal.fire({\r\n            title: 'Error!',\r\n            text: `Error al aprobar la solicitud: ${error.response?.data?.message || error.message}`,\r\n            icon: 'error',\r\n            timer: 3000,\r\n            showConfirmButton: false\r\n          });\r\n          console.error('Error approving solicitud:', error);\r\n        });\r\n    },\r\n    rechazarSolicitud(id_solicitud) {\r\n      if (!this.visualizada) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de rechazarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n      \r\n      let url = '';\r\n      let requestData = {};\r\n      if (this.role === 'jefe_departamento') {\r\n        url = `http://localhost:8000/api/rechazar-solicitud/${id_solicitud}`;\r\n        requestData = { comentarios_jefe: '' };\r\n      } else if (this.role === 'rrhh') {\r\n        url = `http://localhost:8000/api/rechazar-solicitud-rrhh/${id_solicitud}`;\r\n        requestData = { comentarios_rrhh: '' };\r\n      }\r\n\r\n      Swal.fire({\r\n        title: 'Ingrese el motivo para el rechazo:',\r\n        input: 'textarea',\r\n        inputAttributes: {\r\n          'aria-label': 'Ingrese el motivo para el rechazo'\r\n        },\r\n        showCancelButton: true,\r\n        confirmButtonText: 'Rechazar',\r\n        cancelButtonText: 'Cancelar',\r\n        showLoaderOnConfirm: true,\r\n        preConfirm: (comentarios) => {\r\n          if (!comentarios) {\r\n            Swal.showValidationMessage('Debe ingresar un motivo para el rechazo');\r\n          } else {\r\n            if (this.role === 'jefe_departamento') {\r\n              requestData.comentarios_jefe = comentarios;\r\n            } else if (this.role === 'rrhh') {\r\n              requestData.comentarios_rrhh = comentarios;\r\n            }\r\n\r\n            return axios.put(url, requestData)\r\n              .then(response => {\r\n                if (!response.data.success) {\r\n                  throw new Error(response.data.message || 'Error rechazando la solicitud');\r\n                }\r\n                return response.data;\r\n              })\r\n              .catch(error => {\r\n                Swal.showValidationMessage(`Request failed: ${error}`);\r\n              });\r\n          }\r\n        },\r\n        allowOutsideClick: () => !Swal.isLoading()\r\n      }).then((result) => {\r\n        if (result.isConfirmed) {\r\n          Swal.fire({\r\n            title: 'Rechazado!',\r\n            text: 'La solicitud ha sido rechazada.',\r\n            icon: 'success',\r\n            timer: 1000,\r\n            showConfirmButton: false\r\n          }).then(() => {\r\n            this.fetchUserRole(); // Volver a cargar la lista\r\n          });\r\n        }\r\n      });\r\n    },\r\n    visualizarSolicitud(id_solicitud) {\r\n      // Lógica para visualizar la solicitud (puede ser una redirección a una vista detallada o mostrar un modal)\r\n      console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\r\n      this.visualizada = true;\r\n    },\r\n    formatFecha(fecha) {\r\n      if (!fecha) return '';\r\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchUserRole();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n