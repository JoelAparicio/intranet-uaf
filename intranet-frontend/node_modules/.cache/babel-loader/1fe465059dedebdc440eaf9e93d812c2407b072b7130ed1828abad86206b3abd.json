{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  data() {\n    return {\n      solicitudesAceptadas: [],\n      loading: true,\n      role: ''\n    };\n  },\n  methods: {\n    async fetchUserRole() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/roles_usuario');\n        if (response.data.success) {\n          const roles = response.data.roles;\n          if (roles.includes('Jefe de Tecnología') || roles.includes('Jefe de Relaciones Públicas') || roles.includes('Jefe de Administración') || roles.includes('Jefe de Análisis Estratégico') || roles.includes('Jefe de Análisis Operativo') || roles.includes('Jefe de Asesoría Legal') || roles.includes('Jefe de Contact Center') || roles.includes('Jefe de Cooperación Nacional e Internacional')) {\n            this.role = 'jefe_departamento';\n            this.fetchSolicitudesAceptadasDepartamento();\n          } else if (roles.includes('Jefe de Recursos Humanos') || roles.includes('Recursos Humanos')) {\n            this.role = 'rrhh';\n            this.fetchSolicitudesAceptadasRRHH();\n          }\n        }\n      } catch (error) {\n        console.error('Error fetching user role:', error);\n      }\n    },\n    fetchSolicitudesAceptadasDepartamento() {\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-departamento').then(response => {\n        if (response.data.success) {\n          this.solicitudesAceptadas = response.data.data;\n        }\n        this.loading = false;\n      }).catch(error => {\n        console.error('Error fetching solicitudes aceptadas:', error);\n        this.loading = false;\n      });\n    },\n    fetchSolicitudesAceptadasRRHH() {\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-rrhh').then(response => {\n        if (response.data.success) {\n          this.solicitudesAceptadas = response.data.data;\n        }\n        this.loading = false;\n      }).catch(error => {\n        console.error('Error fetching solicitudes aceptadas:', error);\n        this.loading = false;\n      });\n    },\n    formatFecha(fecha) {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    }\n  },\n  mounted() {\n    this.fetchUserRole();\n  }\n};","map":{"version":3,"names":["axios","moment","FontAwesomeIcon","components","data","solicitudesAceptadas","loading","role","methods","fetchUserRole","response","get","success","roles","includes","fetchSolicitudesAceptadasDepartamento","fetchSolicitudesAceptadasRRHH","error","console","then","catch","formatFecha","fecha","format","mounted"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Solicitudes_Aceptadas.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-0\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header\">\r\n        <h4 class=\"text-center\">Solicitudes Aceptadas</h4>\r\n      </div>\r\n      <div class=\"card-body pb-0\" style=\"max-height: 700px; overflow-y: auto;\">\r\n        <table class=\"table table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th>ID Solicitud</th>\r\n              <th>Colaborador</th>\r\n              <th>Tipo de Solicitud</th>\r\n              <th>Fecha de Creación</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-if=\"solicitudesAceptadas.length === 0 && !loading\">\r\n              <td colspan=\"4\" class=\"text-center\">\r\n                No has aceptado solicitudes <font-awesome-icon :icon=\"['far', 'face-smile-wink']\" />\r\n              </td>\r\n            </tr>\r\n            <tr v-else v-for=\"solicitud in solicitudesAceptadas\" :key=\"solicitud.id_solicitud\">\r\n              <td>{{ solicitud.id_solicitud }}</td>\r\n              <td>{{ solicitud.jefe_departamento }}</td>\r\n              <td>{{ 