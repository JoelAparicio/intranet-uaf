\n          }\r\n          this.loading = false;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching solicitudes aceptadas:', error);\r\n          this.loading = false;\r\n        });\r\n    },\r\n    fetchSolicitudesAceptadasRRHH() {\r\n      axios.get('http://localhost:8000/api/solicitudes-aceptadas-rrhh')\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            this.solicitudesAceptadas = response.data.data;\r\n          }\r\n          this.loading = false;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching solicitudes aceptadas:', error);\r\n          this.loading = false;\r\n        });\r\n    },\r\n    formatFecha(fecha) {\r\n      if (!fecha) return '';\r\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchUserRole();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin-top: 20px;\r\n}\r\n.card {\r\n  border: 1px solid #dee2e6;\r\n  border-radius: 5px;\r\n  padding: 15px;\r\n}\r\n.card-header {\r\n  border-bottom: 1px solid #dee2e6;\r\n}\r\nh4 {\r\n  margin-bottom: 0;\r\n}\r\n.table th, .table td {\r\n  vertical-align: middle;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAO,kBAAkB;AACzB,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,oBAAoB,EAAE,EAAE;MACxBC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,yCAAyC,CAAC;QAC3E,IAAID,QAAQ,CAACN,IAAI,CAACQ,OAAO,EAAE;UACzB,MAAMC,KAAI,GAAIH,QAAQ,CAACN,IAAI,CAACS,KAAK;UACjC,IAAIA,KAAK,CAACC,QAAQ,CAAC,oBAAoB,KAAKD,KAAK,CAACC,QAAQ,CAAC,6BAA6B,KACpFD,KAAK,CAACC,QAAQ,CAAC,wBAAwB,KAAKD,KAAK,CAACC,QAAQ,CAAC,8BAA8B,KACzFD,KAAK,CAACC,QAAQ,CAAC,4BAA4B,KAAKD,KAAK,CAACC,QAAQ,CAAC,wBAAwB,KACvFD,KAAK,CAACC,QAAQ,CAAC,wBAAwB,KAAKD,KAAK,CAACC,QAAQ