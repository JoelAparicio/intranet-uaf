, _createElementVNode(\"div\", _hoisted_11, [_hoisted_12, _withDirectives(_createElementVNode(\"select\", {\n      name: \"departamento\",\n      \"onUpdate:modelValue\": _cache[2] || (_cache[2] = $event => _ctx.newBirthday.departamento = $event),\n      id: \"departamento\",\n      class: \"form-control custom-select\"\n    }, [..._hoisted_25], 512 /* NEED_PATCH */), [[_vModelSelect, _ctx.newBirthday.departamento]])]), _createElementVNode(\"div\", _hoisted_26, [_hoisted_27, _withDirectives(_createElementVNode(\"input\", {\n      type: \"date\",\n      class: \"form-control\",\n      id: \"birthday\",\n      \"onUpdate:modelValue\": _cache[3] || (_cache[3] = $event => _ctx.newBirthday.birthday = $event),\n      required: \"\"\n    }, null, 512 /* NEED_PATCH */), [[_vModelText, _ctx.newBirthday.birthday]])]), _hoisted_28], 32 /* NEED_HYDRATION */)])])])])) : _createCommentVNode(\"v-if\", true)]),\n    _: 1 /* STABLE */\n  })])]);\n}","map":{"version":3,"names":["class","key","_createElementVNode","for","value","_hoisted_13","_hoisted_14","_hoisted_15","_hoisted_16","_hoisted_17","_hoisted_18","_hoisted_19","_hoisted_20","_hoisted_21","_hoisted_22","_hoisted_23","_hoisted_24","type","_createElementBlock","_hoisted_1","_hoisted_2","_createVNode","_component_FullCalendar","ref","options","_ctx","calendarOptions","eventContent","_withCtx","arg","_toDisplayString","timeText","event","title","_","_createCommentVNode","_Transition","name","default","showModal","_hoisted_3","_hoisted_4","_hoisted_5","_hoisted_6","_hoisted_7","onClick","_cache","$event","_hoisted_8","onSubmit","_withModifiers","args","addBirthday","_hoisted_9","_hoisted_10","id","newBirthday","nombre","required","_hoisted_11","_hoisted_12","departamento","_hoisted_25","_hoisted_26","_hoisted_27","birthday","_hoisted_28"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Calendario_Birth.vue"],"sourcesContent":["<template>\r\n  <div class=\"container-fluid d-flex p-0\">\r\n    <div class=\"main-content flex-grow-1 p-3\">\r\n      <FullCalendar\r\n        ref=\"calendar\"\r\n        :options=\"calendarOptions\"\r\n      >\r\n        <template v-slot:eventContent=\"arg\">\r\n          <b>{{ arg.timeText }}</b>\r\n          <i>{{ arg.event.title }}</i>\r\n        </template>\r\n      </FullCalendar>\r\n\r\n      <!-- Modal para agregar cumpleaños -->\r\n      <transition name=\"modal\">\r\n        <div v-if=\"showModal\" class=\"modal-overlay\">\r\n          <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\">Agregar Cumpleaños</h5>\r\n                <button type=\"button\" class=\"btn-close\" @click=\"showModal = false\"></button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <form @submit.prevent=\"addBirthday\">\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"nombre\" class=\"form-label\">Nombre</label>\r\n                    <input type=\"text\" class=\"form-control\" id=\"nombre\" v-model=\"newBirthday.nombre\" required>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"departamento\" class=\"form-label\">Departamento</label>\r\n                    <select name=\"departamento\" v-model=\"newBirthday.departamento\" id=\"departamento\" class=\"form-control custom-select\">\r\n                      <option value=\"\">Selecciona un departamento</option>\r\n                      <option value=\"1\">Despacho superior</option>\r\n                      <option value=\"2\">Secretaría</option>\r\n                      <option value=\"3\">Relaciones públicas</option>\r\n                      <option value=\"4\">Administración</option>\r\n                      <option value=\"5\">Recursos humanos</option>\r\n                      <option value=\"6\">Análisis operativo</option>\r\n                      <option value=\"7\">Análisis estratégico</option>\r\n                      <option value=\"8\">Asesoría legal</option>\r\n                      <option value=\"9\">Contact Center</option>\r\n                      <option value=\"10\">Cooperación nacional e internacional</option>\r\n                      <option value=\"11\">Tecnología</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"birthday\" class=\"form-label\">Fecha de Cumpleaños</label>\r\n                    <input type=\"date\" class=\"form-control\" id=\"birthday\" v-model=\"newBirthday.birthday\" required>\r\n                  </div>\r\n                  <button type=\"submit\" class=\"btn btn-primary\">Agregar</button>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </transition>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, ref, onMounted } from 'vue'\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\nimport interactionPlugin from '@fullcalendar/interaction'\r\nimport esLocale from '@fullcalendar/core/locales/es'\r\nimport axios from 'axios'\r\nimport Swal from 'sweetalert2'\r\nimport moment from 'moment'\r\nimport * as XLSX from 'xlsx'\r\n\r\nexport default defineComponent({\r\n  components: {\r\n    FullCalendar,\r\n  },\r\n  setup() {\r\n    const calendarRef = ref(null);\r\n    const showModal = ref(false);\r\n    const newBirthday = ref({\r\n      nombre: '',\r\n      departamento: '',\r\n      birthday: ''\r\n    });\r\n    const currentEvents = ref([]);\r\n    const calendarTitle = ref('');\r\n\r\n    const calendarOptions = {\r\n      plugins: [\r\n        dayGridPlugin,\r\n        interactionPlugin // needed for dateClick\r\n      ],\r\n      headerToolbar: {\r\n        left: 'prev,next today addBirthdayButton',\r\n        center: 'title',\r\n        right: 'colaboradoresButton downloadExcelButton'\r\n      },\r\n      customButtons: {\r\n        addBirthdayButton: {\r\n          text: 'Agregar Cumpleaños',\r\n          click: () => {\r\n            showModal.value = true;\r\n          }\r\n        },\r\n        downloadExcelButton: {\r\n          text: 'Descargar Excel',\r\n          click: () => {\r\n            downloadExcel();\r\n          }\r\n        },\r\n        colaboradoresButton: {\r\n          text: 'Directorio de colaboradores',\r\n          click: () => {\r\n            goToColaboradores();\r\n          }\r\n        }\r\n      },\r\n      initialView: 'dayGridMonth',\r\n      locale: esLocale, // Usar el locale español\r\n      editable: false, // Deshabilitar edición\r\n      selectable: false, // Deshabilitar selección\r\n      selectMirror: true,\r\n      dayMaxEvents: true,\r\n      weekends: true,\r\n      eventClick: handleEventClick,\r\n      eventsSet: handleEvents,\r\n      events: fetchEvents // Fetch events from API\r\n    };\r\n\r\n    onMounted(() => {\r\n      if (calendarRef.value) {\r\n        calendarTitle.value = calendarRef.value.getApi().view.title;\r\n      }\r\n    });\r\n\r\n    function handleEventClick(clickInfo) {\r\n      Swal.fire({\r\n        title: '¿Estás seguro?',\r\n        text: `¿Estás seguro de que deseas eliminar el cumpleaños de ${clickInfo.event.title}?`,\r\n        icon: 'warning',\r\n        showCancelButton: true,\r\n        confirmButtonColor: '#3085d6',\r\n        cancelButtonColor: '#d33',\r\n        confirmButtonText: 'Sí, eliminar',\r\n        cancelButtonText: 'Cancelar'\r\n      }).then((result) => {\r\n        if (result.isConfirmed) {\r\n          axios.put(`http://localhost:8000/api/birthdays/${clickInfo.event.id}`, { deleted: true })\r\n            .then(response => {\r\n              clickInfo.event.remove();\r\n              Swal.fire({\r\n                title: 'Eliminado',\r\n                text: 'El evento ha sido eliminado.',\r\n                icon: 'success',\r\n                timer: 2000,\r\n                showConfirmButton: false\r\n              });\r\n            })\r\n            .catch(error => {\r\n              Swal.fire({\r\n                title: 'Error',\r\n                text: 'Hubo un problema al eliminar el evento.',\r\n                icon: 'error',\r\n                timer: 2000,\r\n                showConfirmButton: false\r\n              });\r\n            });\r\n        }\r\n      });\r\n    }\r\n\r\n    function handleEvents(events) {\r\n      currentEvents.value = events;\r\n      if (calendarRef.value) {\r\n        calendarTitle.value = calendarRef.value.getApi().view.title;\r\n      }\r\n    }\r\n\r\n    function fetchEvents(fetchInfo, successCallback, failureCallback) {\r\n      axios.get('http://localhost:8000/api/birthdays')\r\n        .then(response => {\r\n          const events = response.data.map(event => ({\r\n            id: event.id,\r\n            title: event.nombre,\r\n            start: event.birthday,\r\n            allDay: true\r\n          }));\r\n          successCallback(events);\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching events:', error);\r\n          if (typeof failureCallback === 'function') {\r\n            failureCallback(error);\r\n          }\r\n        });\r\n    }\r\n\r\n    function addBirthday() {\r\n      const formattedBirthday = moment(newBirthday.value.birthday).format('YYYY-MM-DD');\r\n      const birthdayData = {\r\n        ...newBirthday.value,\r\n        birthday: formattedBirthday\r\n      };\r\n\r\n      axios.post('http://localhost:8000/api/addBirthday', birthdayData)\r\n        .then(response => {\r\n          showModal.value = false;\r\n          fetchEvents(\r\n            null,\r\n            (events) => {\r\n              currentEvents.value = events;\r\n            },\r\n            (error) => {\r\n              console.error('Error fetching events:', error);\r\n            }\r\n          );\r\n          Swal.fire({\r\n            title: 'Agregado',\r\n            text: 'El cumpleaños ha sido agregado.',\r\n            icon: 'success',\r\n            timer: 2000,\r\n            showConfirmButton: false\r\n          });\r\n        })\r\n        .catch(error => {\r\n          console.log(birthdayData);\r\n          Swal.fire({\r\n            title: 'Error',\r\n            text: 'Hubo un problema al agregar el cumpleaños.',\r\n            icon: 'error',\r\n            timer: 2000,\r\n            showConfirmButton: false\r\n          });\r\n        });\r\n    }\r\n\r\n    function downloadExcel() {\r\n      if (!calendarRef.value) {\r\n        console.log(calendarTitle.value);\r\n        Swal.fire({\r\n          title: 'Error',\r\n          text: 'No se pudo obtener la referencia del calendario.',\r\n          icon: 'error',\r\n          timer: 2000,\r\n          showConfirmButton: false\r\n        });\r\n        return;\r\n      }\r\n\r\n      const events = currentEvents.value;\r\n      const currentMonth = moment(calendarRef.value.getApi().getDate()).format('MM');\r\n      const filteredEvents = events.filter(event => moment(event.start).format('MM') === currentMonth);\r\n\r\n      const worksheet = XLSX.utils.json_to_sheet(filteredEvents.map(event => ({\r\n        Nombre: event.title,\r\n        Fecha: moment(event.start).format('YYYY-MM-DD')\r\n      })));\r\n      const workbook = XLSX.utils.book_new();\r\n      XLSX.utils.book_append_sheet(workbook, worksheet, 'Cumpleaños');\r\n      XLSX.writeFile(workbook, `Cumpleaños_${calendarTitle.value}.xlsx`);\r\n    }\r\n\r\n    function goToColaboradores() {\r\n      // Lógica para navegar al directorio de colaboradores\r\n      console.log('Navegando al directorio de colaboradores...');\r\n    }\r\n\r\n    return {\r\n      calendarRef,\r\n      showModal,\r\n      newBirthday,\r\n      calendarOptions,\r\n      currentEvents,\r\n      calendarTitle,\r\n      handleEventClick,\r\n      handleEvents,\r\n      fetchEvents,\r\n      addBirthday,\r\n      downloadExcel,\r\n      goToColaboradores\r\n    };\r\n  }\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.container-fluid {\r\n  height: 100vh;\r\n  display: flex;\r\n}\r\n\r\n.main-content {\r\n  flex-grow: 1;\r\n}\r\n\r\n.fc {\r\n  max-width: 100%;\r\n  height: calc(100vh - 120px); /* Ajustar el tamaño del calendario */\r\n  overflow: auto;\r\n}\r\n\r\n/* Estilos para el modal */\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1050;\r\n}\r\n\r\n.modal-dialog {\r\n  max-width: 500px;\r\n  width: 100%;\r\n  margin: 0;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  border-radius: 0.3rem;\r\n  box-shadow: 0 3px 9px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.modal-header {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 1rem 1rem;\r\n  border-bottom: 1px solid #dee2e6;\r\n}\r\n\r\n.modal-title {\r\n  margin-bottom: 0;\r\n  line-height: 1.5;\r\n}\r\n\r\n.btn-close {\r\n  padding: 0.5rem;\r\n  border: none;\r\n}\r\n\r\n.modal-body {\r\n  padding: 1rem;\r\n}\r\n\r\n.modal-enter-active, .modal-leave-active {\r\n  opacity: 1;\r\n  transition: opacity 0.5s ease;\r\n}\r\n\r\n.modal-enter, .modal-leave-to /* .modal-leave-active en versiones anteriores a 2.1.8 */ {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":";;;EACOA,KAAK,EAAC;AAA4B;;EAChCA,KAAK,EAAC;AAA8B;;EAF7CC,GAAA;EAe8BD,KAAK,EAAC;;;EACrBA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAe;;EACnBA,KAAK,EAAC;AAAc;gEACvBE,mBAAA,CAA+C;EAA3CF,KAAK,EAAC;AAAa,GAAC,oBAAkB;;EAGvCA,KAAK,EAAC;AAAY;;EAEdA,KAAK,EAAC;AAAM;iEACfE,mBAAA,CAAqD;EAA9CC,GAAG,EAAC,QAAQ;EAACH,KAAK,EAAC;GAAa,QAAM;;EAG1CA,KAAK,EAAC;AAAM;iEACfE,mBAAA,CAAiE;EAA1DC,GAAG,EAAC,cAAc;EAACH,KAAK,EAAC;GAAa,cAAY;iEAEvDE,mBAAA,CAAoD;EAA5CE,KAAK,EAAC;AAAE,GAAC,4BAA0B;iEAC