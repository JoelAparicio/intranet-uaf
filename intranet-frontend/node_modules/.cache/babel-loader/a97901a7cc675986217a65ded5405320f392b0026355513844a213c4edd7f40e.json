{"ast":null,"code":"import axios from 'axios';\nimport { Modal } from 'bootstrap';\nimport Swal from 'sweetalert2';\nimport moment from 'moment';\nexport default {\n  name: 'Solicitudes',\n  data() {\n    return {\n      solicitudes: [],\n      showModal: false,\n      selectedSolicitud: null,\n      selectedSolicitudTitle: '',\n      solicitud_permiso: {\n        motivo: '',\n        observacion: '',\n        fecha_inicio: '',\n        fecha_fin: ''\n      },\n      solicitud_reincorporacion: {\n        motivo: '',\n        observacion: '',\n        fecha_inicio: ''\n      },\n      solicitud_uso_tiempo: {\n        duracion: '',\n        unidad: 'horas',\n        observacion: '',\n        fecha_inicio: '',\n        fecha_fin: ''\n      },\n      solicitud_vacaciones: {\n        fecha_inicio: '',\n        salario: '',\n        dias: '',\n        fecha_fin: ''\n      },\n      solicitud_horas_extras: {\n        fecha_inicio: '',\n        fecha_fin: '',\n        trabajo_realizado: '',\n        justificacion: '',\n        observacion: ''\n      },\n      motivosDescontables: ['Enfermedad', 'Duelo', 'Matrimonio', 'Nacimiento de hijos', 'Enfermedades de parientes cercanos', 'Eventos academicos puntuales', 'Permisos personales'],\n      motivosNoDescontables: ['Mision oficial', 'Seminarios', 'Otros, especifique'],\n      motivosReincorporacion: ['Licencia con sueldo', 'Licencia sin sueldo', 'Licencia especial', 'Vacaciones'],\n      solicitudModalInstance: null\n    };\n  },\n  created() {\n    this.fetchSolicitudes();\n    this.$nextTick(() => {\n      this.solicitudModalInstance = new Modal(document.getElementById('solicitudModal'));\n    });\n  },\n  methods: {\n    async fetchSolicitudes() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/listar_solicitud');\n        this.solicitudes = response.data;\n      } catch (error) {\n        console.error('Error fetching solicitudes', error);\n      }\n    },\n    handleCardClick(id) {\n      this.selectedSolicitud = id;\n      this.selectedSolicitudTitle = this.getSolicitudTitle(id);\n      this.showModal = true;\n      this.solicitudModalInstance.show();\n    },\n    closeModal() {\n      this.showModal = false;\n      this.solicitudModalInstance.hide();\n    },\n    getSolicitudTitle(id) {\n      const solicitud = this.solicitudes.find(s => s.id_tipo_solicitud === id);\n      return solicitud ? solicitud.tipo_solicitud : '';\n    },\n    validarFechas(fecha_inicio, fecha_fin) {\n      return new Date(fecha_fin) >= new Date(fecha_inicio);\n    },\n    validarPermiso(fecha_inicio, fecha_fin) {\n      const inicio = moment(fecha_inicio, 'YYYY-MM-DD HH:mm');\n      const fin = moment(fecha_fin, 'YYYY-MM-DD HH:mm');\n      const diasSemana = [1, 2, 3, 4, 5];\n      const horaInicioPermitida = moment('08:30', 'HH:mm');\n      const horaFinPermitida = moment('16:00', 'HH:mm');\n      const horaInicio = moment(inicio.format('HH:mm'), 'HH:mm');\n      const horaFin = moment(fin.format('HH:mm'), 'HH:mm');\n      if (!diasSemana.includes(inicio.day()) || !diasSemana.includes(fin.day())) return false;\n      if (horaInicio.isBefore(horaInicioPermitida) || horaInicio.isAfter(horaFinPermitida)) return false;\n      if (horaFin.isBefore(horaInicioPermitida) || horaFin.isAfter(horaFinPermitida)) return false;\n      return true;\n    },\n    validarTiempoCompensatorio(fecha_inicio, fecha_fin, duracion, unidad) {\n      const inicio = moment(fecha_inicio, 'YYYY-MM-DD HH:mm');\n      const fin = moment(fecha_fin, 'YYYY-MM-DD HH:mm');\n      const diff = unidad === 'horas' ? fin.diff(inicio, 'hours') : fin.diff(inicio, 'days');\n      return diff === parseInt(duracion);\n    },\n    formatFecha(fecha) {\n      return moment(fecha).format('YYYY-MM-DD HH:mm');\n    },\n    formatFechaVacaciones(fecha) {\n      return moment(fecha).format('YYYY-MM-DD 00:00');\n    },\n    validarHorario(fecha_inicio, fecha_fin) {\n      const inicio = moment(fecha_inicio, 'YYYY-MM-DD HH:mm');\n      const fin = moment(fecha_fin, 'YYYY-MM-DD HH:mm');\n      return inicio.format('HH:mm') !== fin.format('HH:mm');\n    },\n    validarVacaciones(fecha_inicio, fecha_fin, duracion) {\n      const inicio = moment(fecha_inicio, 'YYYY-MM-DD HH:mm');\n      const fin = moment(fecha_fin, 'YYYY-MM-DD HH:mm');\n      const diffDias = fin.diff(inicio, 'days');\n      return [15, 30].includes(duracion) && diffDias === duracion;\n    },\n    validarHorasExtras(fecha_inicio, fecha_fin) {\n      const inicio = moment(fecha_inicio, 'YYYY-MM-DD HH:mm');\n      const fin = moment(fecha_fin, 'YYYY-MM-DD HH:mm');\n      const mismoDia = inicio.isSame(fin, 'day');\n      const diaSemana = inicio.day();\n      const esSabado = diaSemana === 6;\n      const horaInicioLaboral = moment('08:30', 'HH:mm');\n      const horaFinLaboral = moment('15:59', 'HH:mm');\n      const horaInicio = moment(inicio.format('HH:mm'), 'HH:mm');\n      const horaFin = moment(fin.format('HH:mm'), 'HH:mm');\n      if (!mismoDia) return false;\n      if (esSabado) return true;\n      if (horaInicio.isBetween(horaInicioLaboral, horaFinLaboral, null, '[]')) return false;\n      if (horaFin.isBetween(horaInicioLaboral, horaFinLaboral, null, '[]')) return false;\n      return true;\n    },\n    async enviarSolicitud() {\n      try {\n        let solicitudData;\n        const format = this.formatFecha;\n        if (this.selectedSolicitud === 1) {\n          const inicio = format(this.solicitud_permiso.fecha_inicio);\n          const fin = format(this.solicitud_permiso.fecha_fin);\n          if (!this.validarFechas(inicio, fin) || !this.validarPermiso(inicio, fin)) return;\n          solicitudData = {\n            motivo: this.solicitud_permiso.motivo,\n            observacion: this.solicitud_permiso.observacion,\n            fecha_inicio: inicio,\n            fecha_fin: fin,\n            tipo_solicitud: this.selectedSolicitud\n          };\n        } else if (this.selectedSolicitud === 2) {\n          const inicio = format(this.solicitud_reincorporacion.fecha_inicio);\n          solicitudData = {\n            motivo: this.solicitud_reincorporacion.motivo,\n            observacion: this.solicitud_reincorporacion.observacion,\n            fecha_inicio: inicio,\n            tipo_solicitud: this.selectedSolicitud\n          };\n        } else if (this.selectedSolicitud === 3) {\n          const inicio = format(this.solicitud_uso_tiempo.fecha_inicio);\n          const fin = format(this.solicitud_uso_tiempo.fecha_fin);\n          if (!this.validarTiempoCompensatorio(inicio, fin, this.solicitud_uso_tiempo.duracion, this.solicitud_uso_tiempo.unidad)) return;\n          if (!this.validarPermiso(inicio, fin)) return;\n          solicitudData = {\n            observacion: this.solicitud_uso_tiempo.observacion,\n            fecha_inicio: inicio,\n            fecha_fin: fin,\n            tiempo_utilizado: `${this.solicitud_uso_tiempo.duracion} ${this.solicitud_uso_tiempo.unidad}`,\n            tipo_solicitud: this.selectedSolicitud\n          };\n        } else if (this.selectedSolicitud === 4) {\n          const inicio = this.formatFechaVacaciones(this.solicitud_vacaciones.fecha_inicio);\n          const fin = this.formatFechaVacaciones(this.solicitud_vacaciones.fecha_fin);\n          if (!this.validarFechas(inicio, fin) || !this.validarVacaciones(inicio, fin, this.solicitud_vacaciones.dias)) return;\n          solicitudData = {\n            fecha_inicio: inicio,\n            salario: this.solicitud_vacaciones.salario,\n            dias: this.solicitud_vacaciones.dias,\n            fecha_fin: fin,\n            tipo_solicitud: this.selectedSolicitud\n          };\n        } else if (this.selectedSolicitud === 5) {\n          const inicio = format(this.solicitud_horas_extras.fecha_inicio);\n          const fin = format(this.solicitud_horas_extras.fecha_fin);\n          if (!this.validarFechas(inicio, fin)) return;\n          if (!this.validarHorasExtras(inicio, fin)) return;\n          if (!this.validarHorario(inicio, fin)) return;\n          solicitudData = {\n            fecha_inicio: inicio,\n            fecha_fin: fin,\n            trabajo_realizado: this.solicitud_horas_extras.trabajo_realizado,\n            justificacion: this.solicitud_horas_extras.justificacion,\n            observacion: this.solicitud_horas_extras.observacion,\n            tipo_solicitud: this.selectedSolicitud\n          };\n        }\n        const response = await axios.post('http://localhost:8000/api/insertar_solicitud', solicitudData);\n        if (response.data.status) {\n          Swal.fire({\n            icon: 'success',\n            title: 'Éxito',\n            text: response.data.message,\n            timer: 2000,\n            showConfirmButton: false\n          }).then(() => {\n            this.closeModal();\n            setTimeout(() => {\n              location.reload();\n            }, 700);\n          });\n        }\n      } catch (error) {\n        console.error('Error al enviar la solicitud', error);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'Hubo un problema al enviar la solicitud'\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Modal","Swal","moment","name","data","solicitudes","showModal","selectedSolicitud","selectedSolicitudTitle","solicitud_permiso","motivo","observacion","fecha_inicio","fecha_fin","solicitud_reincorporacion","solicitud_uso_tiempo","duracion","unidad","solicitud_vacaciones","salario","dias","solicitud_horas_extras","trabajo_realizado","justificacion","motivosDescontables","motivosNoDescontables","motivosReincorporacion","solicitudModalInstance","created","fetchSolicitudes","$nextTick","document","getElementById","methods","response","get","error","console","handleCardClick","id","getSolicitudTitle","show","closeModal","hide","solicitud","find","s","id_tipo_solicitud","tipo_solicitud","validarFechas","Date","validarPermiso","inicio","fin","diasSemana","horaInicioPermitida","horaFinPermitida","horaInicio","format","horaFin","includes","day","isBefore","isAfter","validarTiempoCompensatorio","diff","parseInt","formatFecha","fecha","formatFechaVacaciones","validarHorario","validarVacaciones","diffDias","validarHorasExtras","mismoDia","isSame","diaSemana","esSabado","horaInicioLaboral","horaFinLaboral","isBetween","enviarSolicitud","solicitudData","tiempo_utilizado","post","status","fire","icon","title","text","message","timer","showConfirmButton","then","setTimeout","location","reload"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Solicitudes.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"col-md-12\">\r\n      <div class=\"card\">\r\n        <div class=\"card-header\">\r\n          <h5 class=\"mb-3\">Solicitudes</h5>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <p class=\"card-text\">Aquí podrás realizar solicitudes</p>\r\n          <div class=\"row\">\r\n            <div class=\"col-md-4 mb-3\" v-for=\"solicitud in solicitudes\" :key=\"solicitud.id_tipo_solicitud\">\r\n              <div \r\n                class=\"card h-100 shadow-sm clickable-card\" \r\n                @click=\"handleCardClick(solicitud.id_tipo_solicitud)\"\r\n                data-bs-toggle=\"modal\"\r\n                data-bs-target=\"#solicitudModal\"\r\n              >\r\n                <div class=\"card-body d-flex flex-column\">\r\n                  <h5 class=\"card-title\">{{ solicitud.tipo_solicitud }}</h5>\r\n                  <p class=\"card-text\">{{ solicitud.descripcion }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal Vue -->\r\n    <div class=\"modal fade\" id=\"solicitudModal\" tabindex=\"-1\" aria-labelledby=\"solicitudModalLabel\" aria-hidden=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"solicitudModalLabel\">{{ selectedSolicitudTitle }}</h5>\r\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <!-- Formulario dinámico basado en selectedSolicitud -->\r\n            <form v-if=\"selectedSolicitud === 1\" @submit.prevent=\"enviarSolicitud\">\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Motivos descontables</label>\r\n                <div class=\"form-check\" v-for=\"(motivo, index) in motivosDescontables\" :key=\"'desc-' + index\">\r\n                  <input class=\"form-check-input\" type=\"radio\" :id=\"'desc-' + index\" name=\"motivo\" :value=\"motivo\" v-model=\"solicitud_permiso.motivo\">\r\n                  <label class=\"form-check-label\" :for=\"'desc-' + index\">\r\n                    {{ motivo }}\r\n                  </label>\r\n                </div>\r\n                <hr>\r\n                <label class=\"form-label\">Motivos no descontables</label>\r\n                <div class=\"form-check\" v-for=\"(motivo, index) in motivosNoDescontables\" :key=\"'no-desc-' + index\">\r\n                  <input class=\"form-check-input\" type=\"radio\" :id=\"'no-desc-' + index\" name=\"motivo\" :value=\"motivo\" v-model=\"solicitud_permiso.motivo\">\r\n                  <label class=\"form-check-label\" :for=\"'no-desc-' + index\">\r\n                    {{ motivo }}\r\n                  </label>\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"observacion\" class=\"form-label\">Observación</label>\r\n                <textarea class=\"form-control\" id=\"observacion\" v-model=\"solicitud_permiso.observacion\" rows=\"3\" placeholder=\"Ingresa tu observación\"></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_inicio\" class=\"form-label\">Fecha de Inicio</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_inicio\" v-model=\"solicitud_permiso.fecha_inicio\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_fin\" class=\"form-label\">Fecha de Fin</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_fin\" v-model=\"solicitud_permiso.fecha_fin\">\r\n              </div>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Enviar solicitud</button>\r\n            </form>\r\n            <!-- Formulario para selectedSolicitud === 2 -->\r\n            <form v-if=\"selectedSolicitud === 2\" @submit.prevent=\"enviarSolicitud\">\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Motivo</label>\r\n                <div class=\"form-check\" v-for=\"(motivo, index) in motivosReincorporacion\" :key=\"'desc-' + index\">\r\n                  <input class=\"form-check-input\" type=\"radio\" :id=\"'desc-' + index\" name=\"motivo\" :value=\"motivo\" v-model=\"solicitud_reincorporacion.motivo\">\r\n                  <label class=\"form-check-label\" :for=\"'desc-' + index\">\r\n                    {{ motivo }}\r\n                  </label>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"observacion\" class=\"form-label\">Observación</label>\r\n                <textarea class=\"form-control\" id=\"observacion\" v-model=\"solicitud_reincorporacion.observacion\" rows=\"3\" placeholder=\"Ingresa tu observación\"></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_inicio\" class=\"form-label\">Fecha de Inicio</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_inicio\" v-model=\"solicitud_reincorporacion.fecha_inicio\">\r\n              </div>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Enviar solicitud</button>\r\n            </form>\r\n            <!-- Formulario para selectedSolicitud === 3 -->\r\n            <form v-if=\"selectedSolicitud === 3\" @submit.prevent=\"enviarSolicitud\">\r\n              <div class=\"mb-3\">\r\n                <label for=\"duracion\" class=\"form-label\">Tiempo a utilizar</label>\r\n                <div class=\"input-group\">\r\n                  <input type=\"number\" class=\"form-control\" id=\"duracion\" v-model=\"solicitud_uso_tiempo.duracion\" min=\"1\" placeholder=\"Solicito el uso de\">\r\n                  <select class=\"form-select\" v-model=\"solicitud_uso_tiempo.unidad\">\r\n                    <option value=\"horas\">Horas</option>\r\n                    <option value=\"días\">Días</option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"observacion\" class=\"form-label\">Observación</label>\r\n                <textarea class=\"form-control\" id=\"observacion\" v-model=\"solicitud_uso_tiempo.observacion\" rows=\"3\" placeholder=\"Ingresa tu observación\"></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_inicio\" class=\"form-label\">Inicio del tiempo compensatorio</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_inicio\" v-model=\"solicitud_uso_tiempo.fecha_inicio\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_fin\" class=\"form-label\">Finalización del tiempo compensatorio</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_fin\" v-model=\"solicitud_uso_tiempo.fecha_fin\">\r\n              </div>\r\n              \r\n              <button type=\"submit\" class=\"btn btn-primary\">Enviar solicitud</button>\r\n            </form>\r\n            <!-- Formulario para selectedSolicitud === 4 -->\r\n            <form v-if=\"selectedSolicitud === 4\" @submit.prevent=\"enviarSolicitud\">\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_inicio_vacaciones\" class=\"form-label\">Fecha de Inicio</label>\r\n                <input type=\"date\" class=\"form-control\" id=\"fecha_inicio_vacaciones\" v-model=\"solicitud_vacaciones.fecha_inicio\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"salario_vacaciones\" class=\"form-label\">Salario</label>\r\n                <input type=\"number\" class=\"form-control\" id=\"salario_vacaciones\" v-model=\"solicitud_vacaciones.salario\" min=\"0\" step=\"0.01\" placeholder=\"Ingrese su salario mensual\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"dias_vacaciones\" class=\"form-label\">Días</label>\r\n                <input type=\"number\" class=\"form-control\" id=\"dias_vacaciones\" v-model=\"solicitud_vacaciones.dias\" min=\"1\" placeholder=\"Ingrese los días de vacaciones\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_fin_vacaciones\" class=\"form-label\">Fecha de Fin</label>\r\n                <input type=\"date\" class=\"form-control\" id=\"fecha_fin_vacaciones\" v-model=\"solicitud_vacaciones.fecha_fin\">\r\n              </div>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Enviar solicitud</button>\r\n            </form>\r\n            <!-- Formulario para selectedSolicitud === 5 -->\r\n            <form v-if=\"selectedSolicitud === 5\" @submit.prevent=\"enviarSolicitud\">\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_inicio_horas_extras\" class=\"form-label\">Fecha de Inicio</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_inicio_horas_extras\" v-model=\"solicitud_horas_extras.fecha_inicio\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"fecha_fin_horas_extras\" class=\"form-label\">Fecha de Fin</label>\r\n                <input type=\"datetime-local\" class=\"form-control\" id=\"fecha_fin_horas_extras\" v-model=\"solicitud_horas_extras.fecha_fin\">\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"trabajo_realizado\" class=\"form-label\">Trabajo Realizado</label>\r\n                <textarea class=\"form-control\" id=\"trabajo_realizado\" v-model=\"solicitud_horas_extras.trabajo_realizado\" rows=\"3\" placeholder=\"Descripción del trabajo que realizó durante este tiempo\"></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"justificacion\" class=\"form-label\">Justificación</label>\r\n                <textarea class=\"form-control\" id=\"justificacion\" v-model=\"solicitud_horas_extras.justificacion\" rows=\"3\" placeholder=\"Causas que le impidieron realizar las labores antes descritas durante la jornada regular de trabajo.\"></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"observacion_horas_extras\" class=\"form-label\">Observación</label>\r\n                <textarea class=\"form-control\" id=\"observacion_horas_extras\" v-model=\"solicitud_horas_extras.observacion\" rows=\"3\" placeholder=\"Ingresa tu observación\"></textarea>\r\n              </div>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Enviar solicitud</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { Modal } from 'bootstrap'\r\nimport Swal from 'sweetalert2'\r\nimport moment from 'moment'\r\n\r\nexport default {\r\n  name: 'Solicitudes',\r\n  data() {\r\n    return {\r\n      solicitudes: [],\r\n      showModal: false,\r\n      selectedSolicitud: null,\r\n      selectedSolicitudTitle: '',\r\n      solicitud_permiso: {\r\n        motivo: '',\r\n        observacion: '',\r\n        fecha_inicio: '',\r\n        fecha_fin: '',\r\n      },\r\n      solicitud_reincorporacion: {\r\n        motivo: '',\r\n        observacion: '',\r\n        fecha_inicio: ''\r\n      },\r\n      solicitud_uso_tiempo: {\r\n        duracion: '',\r\n        unidad: 'horas',\r\n        observacion: '',\r\n        fecha_inicio: '',\r\n        fecha_fin: ''\r\n      },\r\n      solicitud_vacaciones: {\r\n        fecha_inicio: '',\r\n        salario: '',\r\n        dias: '',\r\n        fecha_fin: '',\r\n      },\r\n      solicitud_horas_extras: {\r\n        fecha_inicio: '',\r\n        fecha_fin: '',\r\n        trabajo_realizado: '',\r\n        justificacion: '',\r\n        observacion: ''\r\n      },\r\n      motivosDescontables: [\r\n        'Enfermedad', 'Duelo', 'Matrimonio', 'Nacimiento de hijos',\r\n        'Enfermedades de parientes cercanos', 'Eventos academicos puntuales', 'Permisos personales'\r\n      ],\r\n      motivosNoDescontables: [\r\n        'Mision oficial', 'Seminarios', 'Otros, especifique'\r\n      ],\r\n      motivosReincorporacion:[\r\n        'Licencia con sueldo', 'Licencia sin sueldo', 'Licencia especial', 'Vacaciones'\r\n      ],\r\n      solicitudModalInstance: null\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchSolicitudes()\r\n    this.$nextTick(() => {\r\n      this.solicitudModalInstance = new Modal(document.getElementById('solicitudModal'))\r\n    })\r\n  },\r\n  methods: {\r\n    async fetchSolicitudes() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/listar_solicitud');\r\n        this.solicitudes = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching solicitudes', error);\r\n      }\r\n    },\r\n    handleCardClick(id) {\r\n      this.selectedSolicitud = id;\r\