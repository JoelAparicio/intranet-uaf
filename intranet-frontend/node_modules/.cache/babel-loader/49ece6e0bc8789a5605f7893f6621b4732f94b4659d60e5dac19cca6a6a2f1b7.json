{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nimport { ref, onMounted } from 'vue';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  setup() {\n    const pendingCount = ref(0);\n    const userRoles = ref([]);\n    const RolesJefesDepartamentos = ['Administrador', 'Jefe de Tecnología', 'Jefe de Relaciones Públicas', 'Jefe de Administración', 'Jefe de Análisis Estratégico', 'Jefe de Análisis Operativo', 'Jefe de Asesoría Legal', 'Jefe de Contact Center', 'Jefe de Cooperación Nacional e Internacional', 'Jefe de Recursos Humanos'];\n    const fetchUserRoles = async () => {\n      try {\n        const token = localStorage.getItem('auth_token');\n        if (!token) {\n          console.error('No se encontró el token en el localStorage');\n          return;\n        }\n        const response = await axios.get('http://localhost:8000/api/roles_usuario', {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        userRoles.value = response.data.roles || [];\n      } catch (error) {\n        console.error('Error fetching user roles:', error);\n      }\n    };\n    const fetchAprobacionesCount = async () => {\n      try {\n        const response = await axios.get('http://localhost:8000/api/listar_aprobaciones_dpto');\n        if (response.data.success) {\n          pendingCount.value = response.data.data.length;\n        }\n      } catch (error) {\n        console.error('Error fetching aprobaciones count:', error);\n      }\n    };\n    onMounted(() => {\n      fetchUserRoles();\n      fetchAprobacionesCount();\n    });\n    const isAdmin = ref(false);\n    const canSeePortalAprobaciones = ref(false);\n    watch(userRoles, newRoles => {\n      isAdmin.value = newRoles.includes('Administrador');\n      canSeePortalAprobaciones.value = newRoles.some(role => rolesThatCanSeeAprobaciones.includes(role));\n    });\n    return {\n      pendingCount,\n      isAdmin,\n      canSeePortalAprobaciones,\n      fetchAprobacionesCount\n    };\n  },\n  methods: {\n    paginaPrincipal() {\n      this.$router.push({\n        name: 'homeinicio'\n      });\n    },\n    paginaMiEspacio() {\n      this.$router.push({\n        name: 'miespacio'\n      });\n    },\n    paginaPortalColaboradores() {\n      this.$router.push({\n        name: 'portalcolaboradores'\n      });\n    },\n    paginaPortalAprobaciones() {\n      this.$router.push({\n        name: 'portalaprobaciones'\n      });\n    },\n    paginaAdministradores() {\n      this.$router.push({\n        name: 'administradores'\n      });\n    },\n    enConstruccion() {\n      Swal.fire({\n        icon: 'warning',\n        title: 'En construcción',\n        text: 'Esta sección está en construcción. Por favor, vuelve más tarde.'\n      });\n    },\n    async cerrarSesion() {\n      try {\n        const token = localStorage.getItem('auth_token');\n        if (!token) {\n          console.error('No se encontró el token en el localStorage');\n          return;\n        }\n        await axios.post('http://localhost:8000/api/logout', {}, {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        localStorage.removeItem('auth_token');\n        this.$router.push({\n          name: 'login'\n        });\n      } catch (error) {\n        console.error('Error al cerrar sesión', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","ref","onMounted","FontAwesomeIcon","components","setup","pendingCount","userRoles","RolesJefesDepartamentos","fetchUserRoles","token","localStorage","getItem","console","error","response","get","headers","value","data","roles","fetchAprobacionesCount","success","length","isAdmin","canSeePortalAprobaciones","watch","newRoles","includes","some","role","rolesThatCanSeeAprobaciones","methods","paginaPrincipal","$router"