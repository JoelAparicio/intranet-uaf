 se encontró el token en el localStorage');\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get('http://localhost:8000/api/roles_usuario', {\r\n          headers: {\r\n            'Authorization': `Bearer ${token}`\r\n          }\r\n        });\r\n\r\n        userRoles.value = response.data.roles || [];\r\n        fetchAprobacionesCount();\r\n      } catch (error) {\r\n        console.error('Error fetching user roles:', error);\r\n      }\r\n    };\r\n\r\n    const fetchAprobacionesCount = async () => {\r\n      try {\r\n        let url = '';\r\n        if (userRoles.value.some(role => RolesJefes.includes(role))) {\r\n          url = 'http://localhost:8000/api/aprobaciones-departamento';\r\n        } else if (userRoles.value.some(role => RolesRRHH.includes(role))) {\r\n          url = 'http://localhost:8000/api/aprobaciones-rrhh';\r\n        }\r\n\r\n        if (url) {\r\n          const response = await axios.get(url);\r\n          if (response.data.success) {\r\n            pendingCount.value = response.data.data.length;\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching aprobaciones count:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchUserRoles();\r\n    });\r\n\r\n    const isAdmin = ref(false);\r\n    const canSeePortalAprobaciones = ref(false);\r\n\r\n    watch(userRoles, (newRoles) => {\r\n      isAdmin.value = newRoles.includes('Administrador');\r\n      canSeePortalAprobaciones.value = newRoles.some(role => [...RolesJefes, ...RolesRRHH].includes(role));\r\n    });\r\n\r\n    return {\r\n      pendingCount,\r\n      isAdmin,\r\n      canSeePortalAprobaciones,\r\n      fetchAprobacionesCount\r\n    };\r\n  },\r\n  methods: {\r\n    paginaPrincipal() {\r\n      this.$router.push({ name: 'homeinicio' });\r\n    },\r\n    paginaMiEspacio() {\r\n      this.$router.push({ name: 'miespacio' });\r\n    },\r\n    paginaPortalColaboradores() {\r\n      this.$router.push({ name: 'portalcolaboradores' });\r\n    },\r\n    paginaPortalAprobaciones() {\r\n      this.$router.push({ name: 'portalaprobaciones' });\r\n    },\r\n    paginaAdministradores() {\r\n      this.$router.push({ name: 'administradores' });\r\n    },\r\n    enConstruccion() {\r\n      Swal.fire({\r\n        icon: 'warning',\r\n        title: 'En construcción',\r\n        text: 'Esta sección está en construcción. Por favor, vuelve más tarde.',\r\n      });\r\n    },\r\n    async cerrarSesion() {\r\n      try {\r\n        const token = localStorage.getItem('auth_token');\r\n        if (!token) {\r\n          console.error('No se encontró el token en el localStorage');\r\n          return;\r\n        }\r\n\r\n        await axios.post('http://localhost:8000/api/logout', {}, {\r\n          headers: {\r\n            'Authorization': `Bearer ${token}`\r\n          }\r\n        });\r\n\r\n        localStorage.removeItem('auth_token');\r\n\r\n        this.$router.push({ name: 'login' });\r\n      } catch (error) {\r\n        console.error('Error al cerrar sesión', error);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.navbar {\r\n  background-color: #1050a9 !important;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.navbar-brand .brand-text {\r\n  font-size: 1.5rem;\r\n  letter-spacing: 1px;\r\n  text-shadow: 2px 2px 4px rgba(0,0,0,0.3);\r\n}\r\n\r\n.nav-link {\r\n  color: rgba(255,255,255,0.8) !important;\r\n  transition: color 0.3s ease, transform 0.3s ease;\r\n}\r\n\r\n.nav-link:hover, .nav-link:focus {\r\n  color: #ffffff !important;\r\n  transform: translateY(-2px);\r\n}\r\n\r\n.btn {\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.btn:hover {\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\r\n}\r\n\r\n.badge {\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n@keyframes pulse {\r\n  0% { transform: scale(1); }\r\n  50% { transform: scale(1.1); }\r\n  100% { transform: scale(1); }\r\n}\r\n\r\n.badge.bg-danger {\r\n  animation