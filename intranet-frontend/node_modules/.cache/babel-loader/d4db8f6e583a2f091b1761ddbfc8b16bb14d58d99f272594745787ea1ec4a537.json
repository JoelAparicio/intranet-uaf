t\" v-model=\"selectedRole\">\r\n          <option value=\"\">Filtrar por Rol</option>\r\n          <option v-for=\"role in roles\" :key=\"role\" :value=\"role\">{{ role }}</option>\r\n        </select>\r\n      </div>\r\n      <table class=\"table table-bordered\">\r\n        <thead>\r\n          <tr>\r\n            <th><input type=\"checkbox\" /></th>\r\n            <th>Nombre</th>\r\n            <th>Email</th>\r\n            <th>Rol</th>\r\n            <th>Status</th>\r\n            <th>Acciones</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"user in filteredUsers\" :key=\"user.id\">\r\n            <td><input type=\"checkbox\" /></td>\r\n            <td>\r\n              <span :style=\"{ backgroundColor: user.color }\" class=\"badge rounded-circle me-2\">{{ user.initials }}</span>\r\n              {{ user.name }}\r\n            </td>\r\n            <td>{{ user.email }}</td>\r\n            <td>\r\n              <span v-if=\"user.role === 'Administrador'\" class=\"text-warning\">ðŸ‘‘</span>\r\n              <span v-if=\"user.role === 'Manager'\" class=\"text-info\">ðŸ‘¥</span>\r\n              {{ user.role }}\r\n            </td>\r\n            <td>\r\n              <span :class=\"user.status === 'Activo' ? 'badge bg-success' : 'badge bg-danger'\">\r\n                {{ user.status }}\r\n              </span>\r\n            </td>\r\n            <td>\r\n              <button class=\"btn btn-link text-dark\"><i class=\"bi bi-trash\"></i></button>\r\n              <button class=\"btn btn-link text-dark\"><i class=\"bi bi-eye\"></i></button>\r\n              <button class=\"btn btn-link text-dark\"><i class=\"bi bi-three-dots\"></i></button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'ListaUsuarioRol',\r\n    data() {\r\n      return {\r\n        searchTerm: '',\r\n        selectedRole: '',\r\n        roles: ['Administrador', 'Manager'],\r\n        users: [\r\n          { id: 1, initials: 'AU', name: 'Admin User 1', email: 'admin@example.com', role: 'Administrador', status: 'Activo', color: '#9b59b6' },\r\n          { id: 2, initials: 'AU', name: 'Admin User 2', email: 'admin2@example.com', role: 'Administrador', status: 'Inactivo', color: '#3498db' },\r\n          { id: 3, initials: 'MU', name: 'Manager User 1', email: 'manager1@example.com', role: 'Manager', status: 'Activo', color: '#1abc9c' },\r\n          { id: 4, initials: 'MU', name: 'Manager User 2', email: 'manager2@example.com', role: 'Manager', status: 'Inactivo', color: '#f1c40f' },\r\n        ],\r\n      };\r\n    },\r\n    computed: {\r\n      filteredUsers() {\r\n        return this.users.filter(user => {\r\n          return (\r\n            (this.searchTerm === '' || user.name.toLowerCase().includes(this.searchTerm.toLowerCase())) &&\r\n            (this.selectedRole === '' || user.role === this.selectedRole)\r\n          );\r\n        });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .table {\r\n    margin-top: 20px;\r\n  }\r\n  .badge {\r\n    padding: 10px;\r\n    font-size: 0.8em;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAoDE,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,CAAC,eAAe,EAAE,SAAS,CAAC;MACnCC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,QAAQ,EAAE,IAAI;QAAEP,IAAI,EAAE,cAAc;QAAEQ,KAAK,EAAE,mBAAmB;QAAEC,IAAI,EAAE,eAAe;QAAEC,MAAM,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAU,CAAC,EACtI;QAAEL,EAAE,EAAE,CAAC;QAAEC,QAAQ,EAAE,IAAI;QAAEP,IAAI,EAAE,cAAc;QAAEQ,KAAK,EAAE,oBAAoB;QAAEC,IAAI,EAAE,eAAe;QAAEC,MAAM,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAU,CAAC,EACzI;QAAEL,EAAE,EAAE,CAAC;QAAEC,QAAQ,EAAE,IAAI;QAAEP,IAAI,EAAE,gBAAgB;QAAEQ,KAAK,EAAE,sBAAsB;QAAEC,IAAI,EAAE,SAAS;QAAEC,MAAM,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAU,CAAC,EACrI;QAAEL,EAAE,EAAE,CAAC;QAAEC,QAAQ,EAAE,IAAI;QAAEP,IAAI,EAAE,gBAAgB;QAAEQ,KAAK,EAAE,sBAAsB;QAAEC,IAAI,EAAE,SAAS;QA