{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapState } from 'vuex';\nexport default {\n  name: 'Login',\n  componentes: {\n    Footer\n  },\n  data() {\n    return {\n      correo_electronico: '',\n      password: '',\n      error: ''\n    };\n  },\n  computed: {\n    ...mapState(['authStatus'])\n  },\n  methods: {\n    ...mapActions(['login']),\n    async submitForm() {\n      try {\n        const user = {\n          correo_electronico: this.correo_electronico,\n          password: this.password\n        };\n        await this.login(user);\n        this.$router.push({\n          name: 'miespacio'\n        }); // después cambiar por homeinicio\n      } catch (error) {\n        this.error = 'Correo electrónico o contraseña incorrectos.';\n      }\n    },\n    redirectToRegister() {\n      this.$router.push({\n        name: 'register'\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapState","name","componentes","Footer","data","correo_electronico","password","error","computed","methods","submitForm","user","login","$router","push","redirectToRegister"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid vh-100 d-flex align-items-center justify-content-center\">\r\n      <div class=\"row justify-content-center w-75\">\r\n        <div class=\"col-md-6 col-lg-4 custom-width\">\r\n          <div class=\"card shadow-lg\">\r\n            <div class=\"card-body p-4\">\r\n              <div class=\"text-center mb-4\">\r\n                <img src=\"../assets/user.png\" alt=\"User Icon\" class=\"img-fluid rounded-circle\" style=\"width: 100px;\">\r\n              </div>\r\n              <h2 class=\"card-title text-center mb-4 fs-3\">Iniciar sesión</h2>\r\n              <form @submit.prevent=\"submitForm\">\r\n                <div class=\"form-floating mb-3\">\r\n                  <input type=\"email\" class=\"form-control\" id=