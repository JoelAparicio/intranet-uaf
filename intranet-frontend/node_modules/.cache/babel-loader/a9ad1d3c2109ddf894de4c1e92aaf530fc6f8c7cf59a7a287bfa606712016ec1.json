licitudes</p>\r\n              <div class=\"row\">\r\n                <div class=\"col-md-4 mb-3\" v-for=\"solicitud in solicitudes\" :key=\"solicitud.id_tipo_solicitud\">\r\n                  <div \r\n                    class=\"card h-100 shadow-sm clickable-card\" \r\n                    @click=\"handleCardClick(solicitud.id_tipo_solicitud)\"\r\n                  >\r\n                    <div class=\"card-body d-flex flex-column\">\r\n                      <h5 class=\"card-title\">{{ solicitud.tipo_solicitud }}</h5>\r\n                      <p class=\"card-text\">{{ solicitud.descripcion }}</p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <PermisoModal v-if=\"showPermisoModal\" :solicitud=\"selectedSolicitud\" @close=\"handleClosePermisoModal\" />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'Solicitudes',\r\n  data() {\r\n    return {\r\n      solicitudes: [],\r\n      solicitud_permiso: {\r\n        motivo: '',\r\n        observacion: '',\r\n        fecha_inicio: '',\r\n        fecha_fin: '',\r\n        tiempo_utilizado: '',\r\n      },\r\n\r\n      \r\n      selectedSolicitud: null, // Store selected solicitud data\r\n      showPermisoModal: false, // Flag for modal visibility\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchSolicitudes()\r\n  },\r\n  methods: {\r\n    async fetchSolicitudes() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/listar_solicitud')\r\n        this.solicitudes = response.data\r\n      } catch (error) {\r\n        console.error('Error fetching solicitudes', error)\r\n      }\r\n    },\r\n    handleCardClick(id) {\r\n      // Lógica para manejar el clic en la tarjeta\r\n      console.log(`Card with ID ${id} clicked`)\r\n      // Puedes redirigir a otra ruta, abrir un modal, etc.\r\n      if (id === 1