{"ast":null,"code":"import axios from 'axios';\nimport moment from 'moment';\nimport 'moment/locale/es';\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\nexport default {\n  components: {\n    FontAwesomeIcon\n  },\n  data() {\n    return {\n      solicitudesAceptadas: [],\n      loading: true\n    };\n  },\n  methods: {\n    fetchSolicitudesAceptadas() {\n      axios.get('http://localhost:8000/api/listar_solicitudes_aceptadas').then(response => {\n        if (response.data.success) {\n          this.solicitudesAceptadas = response.data.data;\n        }\n        this.loading = false;\n      }).catch(error => {\n        console.error('Error fetching solicitudes aceptadas:', error);\n        this.loading = false;\n      });\n    },\n    formatFecha(fecha) {\n      if (!fecha) return '';\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\n    }\n  },\n  mounted() {\n    this.fetchSolicitudesAceptadas();\n  }\n};","map":{"version":3,"names":["axios","moment","FontAwesomeIcon","components","data","solicitudesAceptadas","loading","methods","fetchSolicitudesAceptadas","get","then","response","success","catch","error","console","formatFecha","fecha","format","mounted"],"sources":["C:\\laragon\\www\\intranet-frontend\\src\\components\\Solicitudes_Aceptadas.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-0\">\r\n      <div class=\"card\">\r\n        <div class=\"card-header\">\r\n          <h4 class=\"text-center\">Solicitudes Aceptadas</h4>\r\n        </div>\r\n        <div class=\"card-body pb-0\" style=\"max-height: 440px; overflow-y: auto;\">\r\n          <table class=\"table table-hover\">\r\n            <thead>\r\n              <tr>\r\n                <th>ID Solicitud</th>\r\n                <th>Colaborador</th>\r\n                <th>Tipo de Solicitud</th>\r\n                <th>Fecha de Creaci√≥n</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-if=\"solicitudesAceptadas.length === 0 && !loading\">\r\n                <td colspan=\"4\" class=\"text-center\">\r\n                  No has aceptado solicitudes <font-awesome-icon :icon=\"['far', 'face-smile-wink']\" />\r\n                </td>\r\n              </tr>\r\n              <tr v-else v-for=\"solicitud in solicitudesAceptadas\" :key=\"solicitud.id_solicitud\">\r\n                <td>{{ solicitud.id_solicitud }}</td>\r\n                <td>{{ solicitud.jefe_departamento }}</td>\r\n                <td>{{ solicitud.tipo_solicitud }}</td>\r\n                <td>{{ formatFecha(solicitud.fecha_creacion) }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import moment from 'moment';\r\n  import 'moment/locale/es';\r\n  import { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n  \r\n  export default {\r\n    components: {\r\n      FontAwesomeIcon\r\n    },\r\n    data() {\r\n      return {\r\n        solicitudesAceptadas: [],\r\n        loading: true,\r\n      };\r\n    },\r\n    methods: {\r\n      fetchSolicitudesAceptadas() {\r\n        axios.get('http://localhost:8000/api/listar_solicitudes_aceptadas')\r\n          .then(response => {\r\n            if (response.data.success) {\r\n              this.solicitudesAceptadas = response.data.data;\r\n            }\r\n            this.loading = false;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching solicitudes aceptadas:', error);\r\n            this.loading = false;\r\n          });\r\n      },\r\n      formatFecha(fecha) {\r\n        if (!fecha) return '';\r\n        return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchSolicitudesAceptadas();\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .container {\r\n    margin-top: 20px;\r\n  }\r\n  .card {\r\n    border: 1px solid #dee2e6;\r\n    border-radius: 5px;\r\n    padding: 15px;\r\n  }\r\n  .card-header {\r\n    border-bottom: 1px solid #dee2e6;\r\n  }\r\n  h4 {\r\n    margin-bottom: 0;\r\n  }\r\n  .table th, .table td {\r\n    vertical-align: middle;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAoCE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAO,kBAAkB;AACzB,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,oBAAoB,EAAE,EAAE;MACxBC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,yBAAyBA,CAAA,EAAG;MAC1BR,KAAK,CAACS,GAAG,CAAC,wDAAwD,EAC/DC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACP,IAAI,CAACQ,OAAO,EAAE;UACzB,IAAI,CAACP,oBAAmB,GAAIM,QAAQ,CAACP,IAAI,CAACA,IAAI;QAChD;QACA,IAAI,CAACE,OAAM,GAAI,KAAK;MACtB,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uCAAuC,EAAEA,KAAK,CAAC;QAC7D,IAAI,CAACR,OAAM,GAAI,KAAK;MACtB,CAAC,CAAC;IACN,CAAC;IACDU,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,OAAOhB,MAAM,CAACgB,KAAK,CAAC,CAACC,MAAM,CAAC,qBAAqB,CAAC;IACpD;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACX,yBAAyB,CAAC,CAAC;EAClC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}