ton class=\"btn btn-success btn-sm\" @click=\"aprobarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'check']\" />\r\n                </button>\r\n                <button class=\"btn btn-danger btn-sm\" @click=\"rechazarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'xmark']\" />\r\n                </button>\r\n                <button class=\"btn btn-info btn-sm\" @click=\"visualizarSolicitud(aprobacion.id_solicitud)\">\r\n                  <font-awesome-icon :icon=\"['fas', 'eye']\" />\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport 'moment/locale/es';\r\nimport Swal from 'sweetalert2';\r\nimport { FontAwesomeIcon } from '@fortawesome/vue-fontawesome';\r\n\r\nexport default {\r\n  components: {\r\n    FontAwesomeIcon\r\n  },\r\n  data() {\r\n    return {\r\n      aprobaciones: [],\r\n      loading: true,\r\n      visualizada: false\r\n    };\r\n  },\r\n  methods: {\r\n    fetchAprobaciones() {\r\n      Swal.fire({\r\n        title: 'Cargando...',\r\n        allowOutsideClick: false,\r\n        didOpen: () => {\r\n          Swal.showLoading();\r\n        }\r\n      });\r\n\r\n      axios.get('http://localhost:8000/api/listar_aprobaciones_dpto')\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            this.aprobaciones = response.data.data;\r\n          }\r\n          this.loading = false;\r\n          Swal.close();\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching aprobaciones:', error);\r\n          this.loading = false;\r\n          Swal.close();\r\n        });\r\n    },\r\n    aprobarSolicitud(id_solicitud) {\r\n      if (!this.visualizada) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de aprobarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n      axios.post(`http://localhost:8000/api/aprobar_solicitud/${id_solicitud}`)\r\n        .then(response => {\r\n          if (response.data.success) {\r\n            Swal.fire({\r\n              title: 'Aprobado!',\r\n              text: 'La solicitud ha sido aprobada exitosamente.',\r\n              icon: 'success',\r\n              timer: 1000,\r\n              showConfirmButton: false\r\n            }).then(() => {\r\n              window.location.reload();\r\n            });\r\n          }\r\n        })\r\n        .catch(error => {\r\n          Swal.fire({\r\n            title: 'Error!',\r\n            text: `Error al aprobar la solicitud: ${error.response?.data?.message || error.message}`,\r\n            icon: 'error',\r\n            timer: 3000,\r\n            showConfirmButton: false\r\n          });\r\n          console.error('Error approving solicitud:', error);\r\n        });\r\n    },\r\n    rechazarSolicitud(id_solicitud) {\r\n      if (!this.visualizada) {\r\n        Swal.fire({\r\n          title: 'Atención',\r\n          text: 'Debe visualizar la solicitud antes de rechazarla.',\r\n          icon: 'warning',\r\n          confirmButtonText: 'Ok'\r\n        });\r\n        return;\r\n      }\r\n      Swal.fire({\r\n        title: 'Ingrese el motivo para el rechazo:',\r\n        input: 'textarea',\r\n        inputAttributes: {\r\n          'aria-label': 'Ingrese el motivo para el rechazo'\r\n        },\r\n        showCancelButton: true,\r\n        confirmButtonText: 'Rechazar',\r\n        cancelButtonText: 'Cancelar',\r\n        showLoaderOnConfirm: true,\r\n        preConfirm: (comentarios_jefe) => {\r\n          if (!comentarios_jefe) {\r\n            Swal.showValidationMessage('Debe ingresar un motivo para el rechazo');\r\n          } else {\r\n            return axios.post(`http://localhost:8000/api/rechazar_solicitud/${id_solicitud}`, { comentarios_jefe })\r\n              .then(response => {\r\n                if (!response.data.success) {\r\n                  throw new Error(response.data.message || 'Error rechazando la solicitud');\r\n                }\r\n                return response.data;\r\n              })\r\n              .catch(error => {\r\n                Swal.showValidationMessage(`Request failed: ${error}`);\r\n              });\r\n          }\r\n        },\r\n        allowOutsideClick: () => !Swal.isLoading()\r\n      }).then((result) => {\r\n        if (result.isConfirmed) {\r\n          Swal.fire({\r\n            title: 'Rechazado!',\r\n            text: 'La solicitud ha sido rechazada.',\r\n            icon: 'success',\r\n            timer: 1000,\r\n            showConfirmButton: false\r\n          }).then(() => {\r\n            window.location.reload();\r\n          });\r\n        }\r\n      });\r\n    },\r\n    visualizarSolicitud(id_solicitud) {\r\n      // Lógica para visualizar la solicitud (puede ser una redirección a una vista detallada o mostrar un modal)\r\n      console.log(`Visualizando solicitud con ID: ${id_solicitud}`);\r\n      this.visualizada = true;\r\n    },\r\n    formatFecha(fecha) {\r\n      if (!fecha) return '';\r\n      return moment(fecha).format('MMMM D YYYY, h:mm a');\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchAprobaciones();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin-top: 20px;\r\n}\r\n.card {\r\n  border: 1px solid #dee2e6;\r\n  border-radius: 5px;\r\n  padding: 15px;\r\n}\r\n.card-header {\r\n  border-bottom: 1px solid #dee2e6;\r\n}\r\nh4 {\r\n  margin-bottom: 0;\r\n}\r\n.table th, .table td {\r\n  vertical-align: middle;\r\n}\r\n.btn {\r\n  margin-right: 5px;\r\n}\r\n</style>\r\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAgB;;EACpBA,KAAK,EAAC;AAAM;gEACfC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAa,I,aACtBC,mBAAA,CAA+B,YAA3B,wBAAsB,E;;EAEvBD,KAAK,EAAC,gBAA